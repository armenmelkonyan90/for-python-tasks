+  /**
+   * @description This method is used to update the defi shield
+   * @param body
+   * @param userId
+   * @param id
+   */
+  public async updateDefiShield(
+    id: number,
+    body: UpdateDefiShieldDto,
+    userId: number,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    try {
+      const { platformId, blockchainId, assetId } &#x3D; body;
+
+      const defiShield &#x3D; await this._defiShieldRepository.findOne({
+        id,
+        userId,
       });
-      if (assetName || assetSymbol) {
-        throw buildResponse(false, { message: &#x27;Asset already exists&#x27; });
+      if (!defiShield) {
+        throw buildResponse(false, { message: &#x27;Defi shield not found&#x27; });
+      }
+
+      const asset &#x3D; await this._assetsRepository.findOne(assetId);
+      if (!asset) {
+        throw buildResponse(false, { message: &#x27;Asset not found&#x27; });
+      }
+      const blockchain &#x3D; await this._blockchainRepository.findOne(blockchainId);
+      if (!blockchain) {
+        throw buildResponse(false, { message: &#x27;Blockchain not found&#x27; });
+      }
+      const platform &#x3D; await this._platformRepository.findOne(platformId);
+      if (!platform) {
+        throw buildResponse(false, { message: &#x27;Platform not found&#x27; });
+      }
+
+      if (platformId || blockchainId || assetId) {
+        const connectionExists &#x3D; await this._platformRepository.findOne({
+          where: {
+            id: platformId ? platformId : defiShield.platformId,
+            blockchainId: blockchainId ? blockchainId : defiShield.blockchainId,
+            assetId: assetId ? assetId : defiShield.assetId,
+          },
+        });
+        if (!connectionExists) {
+          throw buildResponse(false, {
+            message: &#x27;No such defi shield connection&#x27;,
+          });
+        }
       }
 
-      const assets &#x3D; await this._assetsRepository.save(body.asset);
-      const blockchain &#x3D; await this._blockchainRepository.save({
-        ...body.blockchain,
-        assetId: assets.id,
+      await this._defiShieldRepository.update(id, body);
+
+      return buildResponse(true) as GeneralResponse;
+    } catch (e) {
+      throw e;
+    }
+  }

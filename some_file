s/common';\nimport { buildResponse } from '@common/helpers';\n\n@Injectable()\nexport class Trim implements PipeTransform {\n  public isObj(obj: any): boolean {\n    return typeof obj === 'object' && obj !== null;\n  }\n\n  private trim(values) {\n    Object.keys(values).forEach((key) => {\n      if (key !== 'password') {\n        if (this.isObj(values[key])) {\n          values[key] = this.trim(values[key]);\n        } else {\n          if (typeof values[key] === 'string') {\n            values[key] = values[key].trim();\n          }\n        }\n      }\n    });\n    return values;\n  }\n\n  transform(values: any, metadata: ArgumentMetadata) {\n    const { type } = metadata;\n    const types = ['query', 'param', 'custom'];\n    if (this.isObj(values) && type === 'body') {\n      return this.trim(values);\n    } else if (types.includes(type)) {\n      return values;\n    }\n\n    throw buildResponse(false, ['Validation failed']);\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"entities/TwoFactorAuth.html":{"url":"entities/TwoFactorAuth.html","title":"entity - TwoFactorAuth","body":"\n                   \n\n\n\n\n\n\n\n  Entities\n  TwoFactorAuth\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/database/entities/2fa.entity.ts\n        \n\n\n\n\n            \n    Index\n    \n        \n                \n                    \n                        Properties\n                    \n                \n                \n                    \n                        \n                            \n                                createdAt\n                            \n                            \n                                id\n                            \n                            \n                                qrScann
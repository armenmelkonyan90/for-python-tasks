 } from '@common/helpers';\n\n@Injectable()\nexport class S3BucketService {\n  AWS_S3_BUCKET = process.env.AWS_S3_BUCKET;\n  s3 = new S3({\n    accessKeyId: process.env.AWS_ACCESS_KEY_ID,\n    secretAccessKey: process.env.AWS_SECRET_ACCESS_KEY,\n  });\n\n  /**\n   * Upload file to S3 bucket\n   * @param file\n   * @param filePath\n   */\n  public async uploadFile(file, filePath: string) {\n    try {\n      const { originalname } = file;\n      const ext = extname(originalname);\n      const { name: originalName } = parse(originalname);\n      const fileName = `${originalName.replace(/\\s/g, '')}_${uuid()}${ext}`;\n\n      return await this.s3_upload(\n        file.buffer,\n        this.AWS_S3_BUCKET,\n        filePath + fileName,\n        file.mimetype,\n      );\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * Upload file to S3 bucket\n   * @param file\n   * @param bucket\n   * @param name\n   * @param mimetype\n   * @private\n   */\n  private async s3_upload(file, bucket, name, mimetype) {\n    const params = {\n      Bucket: bucket,\n      Key: String(name),\n      Body: file,\n      ACL: 'private',\n      ContentType: mimetype,\n      ContentDisposition: 'inline',\n      CreateBucketConfiguration: {\n        LocationConstraint: 'eu-west-1',\n      },\n    };\n\n    try {\n      return await this.s3.upload(params).promise();\n    } catch (e) {\n      throw buildResponse(false, { message: ['Error uploading file'] });\n    }\n  }\n\n  /**\n   * Delete file from S3 bucket\n   * @param prefix\n   */\n  public async removeFile(prefix: string) {\n    let currentData;\n    try {\n      return await this.s3\n        .listObjects({\n          Bucket: this.AWS_S3_BUCKET,\n          Prefix: prefix,\n        })\n        .promise()\n        .then(async (data) => {\n          currentData = data;\n          if (data.Contents.length === 0) {\n            return true;\n          }\n\n          const params = {\n            Bucket: this.AWS_S3_BUCKET,\n            Delete: {\n              Objects: [],\n            },\n          };\n\n          currentData.Contents.forEach((content) => {\n            params.Delete.Objects.push({ Key: content.Key });\n          });\n\n          return await this.s3.deleteObjects(params).promise();\n        })\n        .then(() => {\n          if (currentData.Contents.length === 1000) {\n            this.removeFile(prefix);\n          } else {\n            return true;\n          }\n        });\n    } catch (e) {\n      throw buildResponse(false, { message: ['Error deleting file'] });\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"classes/SaveFile.html":{"url":"classes/SaveFile.html","title":"class - SaveFile","body":"\n                   \n\n\n\n\n\n\n\n\n\n\n\n\n\n  Classes\n  SaveFile\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/helpers/save-file.ts\n        \n\n\n\n\n\n\n            \n    Index\n    \n        \n\n                \n                    \n                        Methods\n                    \n                \n                \n                    \n                        \n                            \n                                    Static\n                                removeFile\n                            \n                            \n                                    Static\n                                saveFile\n                            \n                        \n                    \n                \n\n\n\n\n\n        \n    \n\n\n\n\n            \n    \n    \n        Methods\n    \n    \n        \n            \n                \n                    \n                    \n                        Static\n                        removeFile\n                        \n                    \n               
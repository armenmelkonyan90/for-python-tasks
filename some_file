\n} from '@nestjs/common';\nimport { Request } from 'express';\nimport { TwoFactorAuthenticationService } from './two-factor-authentication.service';\nimport { AuthUserDto } from '../auth/dto';\nimport { AppThrottlerGuard } from '@common/guards/throttler.guard';\nimport { buildResponse } from '@common/helpers';\nimport { GeneralResponse } from '@common/models';\nimport { ChangeQrCodeDto } from './dto/change-qr-code';\nimport * as bcrypt from 'bcrypt';\nimport { User } from '@common/decorators';\nimport { InjectRepository } from '@nestjs/typeorm';\nimport { Users } from '@common/database/entities';\nimport { Repository } from 'typeorm';\n\n/**\n * Controller for two-factor authentication\n */\n@UseGuards(AppThrottlerGuard)\n@Controller('2fa')\nexport class TwoFactorAuthenticationController {\n  constructor(\n    @InjectRepository(Users)\n    private _usersRepository: Repository,\n    private readonly twoFactorAuthenticationService: TwoFactorAuthenticationService,\n  ) {}\n\n  /**\n   * Generate a new two-factor authentication qr code\n   * @param req\n   * @param body\n   */\n  @Post('generate')\n  public async generateCodeFor2Fa(\n    @Req() req: Request,\n    @Body() body: GenerateTwoFactorTypeDto,\n  ): Promise | HttpException> {\n    try {\n      const { type } = body;\n      console.log(req.user);\n      const { otpAuthUrl, secret } =\n        await this.twoFactorAuthenticationService.generateTwoFactorAuthentication(\n          req.user as AuthUserDto,\n        );\n\n      if (type === GenerateTwoFactorTypeEnum.QR) {\n        return buildResponse(true, {\n          secret: await this.twoFactorAuthenticationService.pipeQrCodeStream(\n            otpAuthUrl,\n          ),\n        });\n      } else {\n        return buildResponse(true, { secret });\n      }\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * Generate a new two-factor authentication qr code\n   * @param req\n   * @param body\n   * @param user\n   */\n  @Post('change-qr')\n  public async changeQrCode(\n    @Req() req: Request,\n    @Body() body: ChangeQrCodeDto,\n    @User() user: AuthUserDto,\n  ): Promise | HttpException> {\n    try {\n      const { type, password, code } = body;\n\n      const userExist = await this._usersRepository\n        .createQueryBuilder('users')\n        .where('users.id = :id', { id: user.id })\n        .addSelect(['users.password'])\n        .getOne();\n\n      if (!userExist || !bcrypt.compareSync(password, userExist.password)) {\n        throw buildResponse(false, { message: [`Wrong password`] });\n      }\n\n      await this.twoFactorAuthenticationService.authenticateTwoFactorCode(\n        code,\n        user,\n      );\n\n      const { otpAuthUrl, secret } =\n        await this.twoFactorAuthenticationService.generateTwoFactorAuthentication(\n          req.user as AuthUserDto,\n        );\n\n      if (type === GenerateTwoFactorTypeEnum.QR) {\n        return buildResponse(true, {\n          secret: await this.twoFactorAuthenticationService.pipeQrCodeStream(\n            otpAuthUrl,\n          ),\n        });\n      } else {\n        return buildResponse(true, { secret });\n      }\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @Description: 2FA verify status for client side\n   * @param id\n   */\n  @Get('verify')\n  public get2faVerifyStatus(\n    @User('id') id: number,\n  ): Promise | HttpException> {\n    return this.twoFactorAuthenticationService.getTwoFactorAuthenticationStatus(\n      Number(id),\n    );\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"modules/TwoFactorAuthenticationModule.html":{"url":"modules/TwoFactorAuthenticationModule.html","title":"module - TwoFactorAuthenticationModule","body":"\n                   \n\n\n\n\n    Modules\n    TwoFactorAuthenticationModule\n\n\n\n    \n        \n\n\n\n\n\ndependencies\n\ndependencies\n\ncluster_TwoFactorAuthentica
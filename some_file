   \n                        Returns :         Promise\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n\n\n    \n\n\n    \n        import { HttpException, Injectable } from '@nestjs/common';\nimport { InjectRepository } from '@nestjs/typeorm';\nimport { Assets, Blockchain, Platform } from '@common/database/entities';\nimport { Repository } from 'typeorm';\nimport { buildResponse } from '@common/helpers';\nimport { GeneralResponse } from '@common/models';\nimport { AssetDto, DefiShieldDefaultsDto } from './dto/defishield.dto';\nimport {\n  UpdateAssetDto,\n  UpdateDefiShieldDto,\n} from './dto/update-defishield.dto';\nimport { DeleteDefishieldDto } from './dto/delete-defishield.dto';\nimport { uniqBy } from 'lodash';\n\n@Injectable()\nexport class DefishieldDefaultsService {\n  constructor(\n    @InjectRepository(Assets)\n    private _assetsRepository: Repository,\n    @InjectRepository(Blockchain)\n    private _blockchainRepository: Repository,\n    @InjectRepository(Platform)\n    private _platformRepository: Repository,\n  ) {}\n\n  /**\n   * @description This method is used to get the getAssets\n   */\n  public async getAssets(): Promise {\n    try {\n      let assets = await this._assetsRepository.find();\n      assets = uniqBy(assets, 'name');\n\n      return buildResponse(true, assets) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to get the getBlockchains\n   * @param assetId\n   */\n  public async getBlockchains(\n    assetId: number,\n  ): Promise {\n    try {\n      let blockchains = await this._blockchainRepository.find({ assetId });\n      if (!blockchains) {\n        throw buildResponse(false, { message: ['Blockchain not found'] });\n      }\n\n      blockchains = uniqBy(blockchains, 'name');\n      return buildResponse(true, blockchains) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to get the getPlatforms\n   * @param assetId\n   * @param blockchainName\n   */\n  public async getPlatforms(\n    assetId: number,\n    blockchainName: string,\n  ): Promise {\n    try {\n      let platforms = await this._platformRepository.find({\n        relations: ['blockchainId'],\n        where: { assetId, blockchainId: { name: blockchainName } },\n      });\n      if (!platforms) {\n        throw buildResponse(false, { message: ['Platform not found'] });\n      }\n      platforms = uniqBy(platforms, 'name');\n\n      return buildResponse(true, platforms) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to get the connections\n   */\n  public async getConnections(): Promise {\n    try {\n      const platforms = await this._platformRepository\n        .createQueryBuilder('platform')\n        .innerJoinAndSelect('platform.blockchainId', 'blockchain')\n        .innerJoinAndSelect('platform.assetId', 'asset')\n        .getMany();\n\n      return buildResponse(true, platforms) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to create the risks\n   * @param body\n   */\n  public async createAsset(\n    body: AssetDto,\n  ): Promise {\n    try {\n      const assetName = await this._assetsRepository.findOne({\n        name: body.name,\n      });\n      const assetSymbol = await this._assetsRepository.findOne({\n        symbol: body.symbol,\n      });\n      if (assetName || assetSymbol) {\n        throw buildResponse(false, { message: ['Asset already exists'] });\n      }\n\n      const assets = await this._assetsRepository.save(body);\n\n      return buildResponse(true, { assets }) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  public async createBlockchainAndPlatform(\n    body: DefiShieldDefaultsDto,\n  ): Promise {\n    try {\n      const asset = await this._assetsRepository.findOne({ id: body.assetId });\n      if (!asset) {\n        throw buildResponse(false, { message: ['Asset not found'] });\n      }\n\n      const platformExist = await this._platformRepository.findOne({\n        relations: ['blockchainId'],\n        where: {\n          name: body.platform.name,\n          blockchainId: { name: body.blockchain.name },\n          assetId: body.assetId,\n        },\n      });\n      if (platformExist) {\n        throw buildResponse(false, { message: ['Connection already exists'] });\n      }\n\n      let platform;\n      const blockchain = await this._blockchainRepository.save({\n        ...body.blockchain,\n        assetId: body.assetId,\n      });\n      if (blockchain) {\n        platform = await this._platformRepository.save({\n          ...body.platform,\n          blockchainId: blockchain.id,\n          assetId: body.assetId,\n        });\n      }\n\n      return buildResponse(true, {\n        blockchain,\n        platform,\n      }) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to get the defi shield data\n   */\n  public async getDefiShieldDefaults(): Promise {\n    try {\n      const data = await this._assetsRepository.find({\n        relations: ['blockchain', 'blockchain.platform'],\n      });\n\n      return buildResponse(true, { data }) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to update the asset\n   */\n  public async updateAsset(\n    id: number,\n    body: UpdateAssetDto,\n  ): Promise {\n    try {\n      const asset = await this._assetsRepository.findOne(id);\n      if (!asset) {\n        throw buildResponse(false, { message: ['Asset not found'] });\n      }\n      const assetName = await this._assetsRepository.findOne({\n        name: body.name,\n      });\n      const assetSymbol = await this._assetsRepository.findOne({\n        symbol: body.symbol,\n      });\n      if (assetName || assetSymbol) {\n        throw buildResponse(false, { message: ['Asset already exists'] });\n      }\n\n      await this._assetsRepository.update(id, body);\n\n      return buildResponse(true) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to update the blockchain and platform\n   */\n\n  public async updateBlockchainOrPlatform(\n    body: UpdateDefiShieldDto,\n  ): Promise {\n    try {\n      if (body.blockchain) {\n        const blockchain = await this._blockchainRepository.findOne(\n          body.blockchain.id,\n        );\n        if (!blockchain) {\n          throw buildResponse(false, { message: ['Blockchain not found'] });\n        }\n\n        await this._blockchainRepository.update(\n          body.blockchain.id,\n          body.blockchain,\n        );\n      }\n\n      if (body.platform) {\n        const platform = await this._platformRepository.findOne(\n          body.platform.id,\n        );\n        if (!platform) {\n          throw buildResponse(false, { message: ['Platform not found'] });\n        }\n\n        await this._platformRepository.update(body.platform.id, body.platform);\n      }\n\n      return buildResponse(true) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to delete the asset\n   */\n  public async deleteAsset(\n    id: number,\n  ): Promise {\n    try {\n      const asset = await this._assetsRepository.findOne(id);\n      if (!asset) {\n        throw buildResponse(false, { message: ['Asset not found'] });\n      }\n\n      await this._assetsRepository.delete(id);\n\n      return buildResponse(true) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to delete the blockchain or platform\n   * @param query\n   */\n  public async deleteBlockchainOrPlatform(\n    query: DeleteDefishieldDto,\n  ): Promise {\n    try {\n      if (query.blockchainId) {\n        const blockchain = await this._blockchainRepository.findOne(\n          query.blockchainId,\n        );\n        if (!blockchain) {\n          throw buildResponse(false, { message: ['Blockchain not found'] });\n        }\n\n        await this._blockchainRepository.delete(query.blockchainId);\n      }\n\n      if (query.platformId) {\n        const platform = await this._platformRepository.findOne(\n          query.platformId,\n        );\n        if (!platform) {\n          throw buildResponse(false, { message: ['Platform not found'] });\n        }\n\n        await this._platformRepository.delete(query.platformId);\n      }\n\n      return buildResponse(true) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"classes/DeleteDefishieldDto.html":{"url":"classes/DeleteDefishieldDto.html","title":"class - DeleteDefishieldDto","body":"\n                   \n\n\n\n\n\n\n\n\n\n\n\n\n\n  Classes\n  DeleteDefishieldDto\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/resources/defishield/defishield-defaults/dto/delete-defishield.dto.ts\n        \n\n\n\n\n\n\n            \n    Index\n    \n        \n                \n                    \n                        Properties\n                    \n                \n                \n                    \n                        \n                            \n                                    Optional\n                                blockchainId\n                            \n                            \n                                    Optional\n                                platformId\n                            \n                        \n                    \n              
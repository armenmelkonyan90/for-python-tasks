{}\n\n  /**\n   * Get all users\n   */\n  public async findAll(): Promise | HttpException> {\n    try {\n      const users = await this._users.find({\n        relations: ['userSettings', 'alertServices'],\n      });\n\n      return buildResponse(true, users);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * Update user\n   * @param id\n   * @param updateUserDto\n   * @param avatar\n   */\n  public async update(\n    id: number,\n    updateUserDto: UpdateUserDto,\n    avatar: Express.Multer.File,\n  ): Promise {\n    try {\n      const user = await this._users.findOne(id);\n      if (!user) {\n        throw buildResponse(false, { message: ['User not found'] });\n      }\n\n      if (avatar) {\n        if (user.avatar) {\n          await this._s3bucket.removeFile(`avatars/${id}/`);\n        }\n\n        const changedImage = await sharp(avatar.buffer)\n          .resize(350, 350, {\n            fit: 'contain',\n            gravity: 'center',\n          })\n          .toBuffer();\n\n        const fileData = await this._s3bucket.uploadFile(\n          changedImage,\n          avatar,\n          `avatars/${id}/`,\n        );\n        await this._users.update(id, { avatar: fileData.Location });\n      }\n\n      await this._users.update(id, updateUserDto);\n      return buildResponse(true);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * Delete user\n   * @param email\n   */\n  public async delete(email: string): Promise {\n    try {\n      const user = await this._users.findOne({ email });\n      if (!user) {\n        throw buildResponse(false, { message: ['User not found'] });\n      }\n      await this._users.delete({ email });\n      return buildResponse(true);\n    } catch (e) {\n      throw e;\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"entities/UserSettings.html":{"url":"entities/UserSettings.html","title
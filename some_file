     }
   }
 
+  public async refreshToken(
+    body: RefreshTokenDto,
+  ): Promise&lt;GeneralResponse&lt;AccessTokenDto&gt; | HttpException&gt; {
+    try {
+      const { refresh_token } &#x3D; body;
+      const user &#x3D; await this._usersRepository.findOne({
+        refreshToken: refresh_token,
+      });
+
+      if (!user) {
+        throw buildResponse(false, {
+          message: [&#x60;User with such credentials was not found&#x60;],
+        });
+      }
+
+      this._jwtService.verify(refresh_token || &#x27;&#x27;);
+      const decodedUser &#x3D; this._jwtService.decode(refresh_token, {
+        json: true,
+      });
+      const { id } &#x3D; decodedUser as Users;
+      if (id !&#x3D;&#x3D; user.id) {
+        throw buildResponse(false, {
+          message: [&#x60;Refresh token is invalid&#x60;],
+        });
+      }
+
+      const payload: Partial&lt;AuthUserPayloadDto&gt; &#x3D; {
+        ...user,
+        isSecondFactorAuthenticated: true,
+        verified: LoginUsersStatusEnum.ACTIVE,
+        status_id: UserStatusEnum.VERIFIED,
+      };
+
+      const access_token: string &#x3D; this._jwtService.sign(payload, {
+        secret: JWT_OPTIONS.secretKey,
+        expiresIn: JWT_OPTIONS.expiresIn,
+      });
+
+      const refreshToken: string &#x3D; this._jwtService.sign(payload, {
+        secret: JWT_OPTIONS.secretKey,
+        expiresIn: JWT_OPTIONS.refreshExpiresIn,
+      });
+
+      await this._usersRepository.update({ id }, { refreshToken });
+
+      const accessToken: AccessTokenDto &#x3D; {
+        access_token,
+        refresh_token: refreshToken,
+        set_verification: LoginUsersStatusEnum.ACTIVE,
+        verification_type: UserVerificationTypesEnum.GOOGLE,
+        status_id: UserStatusEnum.VERIFIED,
+      };
+
+      return buildResponse(true, accessToken) as GeneralResponse;
+    } catch (e) {
+      throw e;
+    }
+  }
+
+  /**
+   * @description Login admin
+   * @param body
+   */
+  public async loginAdmin(
+    body: LoginDTO,
+  ): Promise&lt;GeneralResponse&lt;AccessTokenDto&gt; | HttpException&gt; {
+    try {
+      const { email, password } &#x3D; body;
+
+      const user &#x3D; await this._usersRepository
+        .createQueryBuilder(&#x27;users&#x27;)
+        .where(&#x27;users.email &#x3D; :email&#x27;, { email: email.toLocaleLowerCase() })
+        .addSelect([&#x27;users.password&#x27;])
+        .getOne();
+
+      if (
+        !user ||
+        !user.password ||
+        !bcrypt.compareSync(password, user.password) ||
+        user.role !&#x3D;&#x3D; RoleTypes.SUPERADMIN
+      ) {
+        throw buildResponse(false, {
+          message: [&#x60;Admin with such credentials was not found&#x60;],
+        });
+      }
+
+      const access_token: string &#x3D; this._jwtService.sign(
+        { ...omit(user, [&#x27;password&#x27;]) },
+        { expiresIn: JWT_OPTIONS.expiresIn },
+      );
+
+      const accessToken: AdminAccessTokenDto &#x3D; {
+        access_token,
+        set_verification: LoginUsersStatusEnum.ACTIVE,
+      };
+
+      return buildResponse(true, accessToken) as GeneralResponse;
+    } catch (e) {
+      throw e;
+    }
+  }
+
   /**
    * @description Set user 2fa secret
    * @param user
@@ -1319,22 +1564,6 @@ export class AuthService {

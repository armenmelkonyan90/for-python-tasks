+  public async alertByTimes(
+    defiShield: any,
+    defiShieldGeneral: any,
+  ): Promise&lt;void&gt; {
+    try {
+      if (defiShield.alertSend) return;
+      await this._defiShieldRepository.update(defiShield.id, {
+        alertSend: true,
+      });
+
+      const jobFunc &#x3D; async () &#x3D;&gt; {
+        return await this.sendAlertEmail(defiShield, defiShieldGeneral);
+      };
+      await jobFunc();
+
+      const job &#x3D; new CronJob(&#x27;0 */15 * * * *&#x27;, async () &#x3D;&gt; {
+        await jobFunc();
+        this.schedulerRegistry.deleteCronJob(&#x27;send-15-min&#x27;);
+      });
+      this.schedulerRegistry.addCronJob(&#x27;send-15-min&#x27;, job);
+      job.start();
+
+      const job2 &#x3D; new CronJob(&#x27;0 0 * * * *&#x27;, async () &#x3D;&gt; {
+        await jobFunc();
+        this.schedulerRegistry.deleteCronJob(&#x27;send-1-hour&#x27;);
+      });
+      this.schedulerRegistry.addCronJob(&#x27;send-1-hour&#x27;, job2);
+      job2.start();
+
+      const job3 &#x3D; new CronJob(&#x27;0 0 0 * * *&#x27;, async () &#x3D;&gt; {
+        await jobFunc();
+        await this._defiShieldRepository.update(defiShield.id, {
+          alertSend: false,
+        });
+        this.schedulerRegistry.deleteCronJob(&#x27;send-24-hours&#x27;);
+      });
+      this.schedulerRegistry.addCronJob(&#x27;send-24-hours&#x27;, job3);
+      job3.start();
+    } catch (e) {
+      return e;
+    }
+  }

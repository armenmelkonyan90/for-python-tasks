+    <div class="tab-pane fade  tab-source-code" id="c-source">
+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import { Injectable } from &#x27;@nestjs/common&#x27;;
+import axios from &#x27;axios&#x27;;
+import { axiosErrorHandler } from &#x27;@common/helpers&#x27;;
+import { ApiServiceData } from &#x27;@common/interface&#x27;;
+
+/**
+ * Api Service
+ */
+@Injectable()
+export class MicroserviceApi {
+  /**
+   * Request to microservice api
+   * @param token
+   * @param method
+   * @param url
+   * @param data
+   */
+  public async request(
+    token: string,
+    method: &#x27;GET&#x27; | &#x27;POST&#x27; | &#x27;PUT&#x27; | &#x27;DELETE&#x27;,
+    url: string,
+    data?: ApiServiceData,
+  ): Promise&lt;any&gt; {
+    try {
+      const { data: response } &#x3D; await axios({
+        baseURL: &#x60;${process.env.MICRO_SERVICE_URL}&#x60;,
+        method: &#x27;GET&#x27;,
+        url,
+        headers: {
+          authorization: token,
+        },
+        ...data,
+      }).catch((e) &#x3D;&gt; {
+        throw axiosErrorHandler(e);
+      });
+
+      return response;
+    } catch (e) {
+      throw e;
+    }
+  }
+}
+</code></pre>
+    </div>
+
+</div>

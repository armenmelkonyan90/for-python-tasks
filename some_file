+
+export class UpdateDefiShieldDto {
+  @IsOptional()
+  @IsNumber()
+  assetId?: number;
+
+  @IsOptional()
+  @IsNumber()
+  blockchainId?: number;
+
+  @IsOptional()
+  @IsNumber()
+  platformId?: number;
+
+  @IsOptional()
+  @IsNumber()
+  @Min(0)
+  @Max(100)
+  alert?: number;
+
+  @IsOptional()
+  @IsNumber()
+  autoUnstacking?: number;
+}

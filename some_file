+    <div class="tab-pane fade  tab-source-code" id="c-source">
+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import { Injectable } from &#x27;@nestjs/common&#x27;;
+import axios from &#x27;axios&#x27;;
+import { axiosErrorHandler } from &#x27;@common/helpers&#x27;;
+
+/**
+ * Api Service
+ */
+@Injectable()
+export class CoinGeckoApi {
+  /**
+   * Get token price
+   * @param name
+   */
+  public async getTokenPrice(name: string): Promise&lt;any&gt; {
+    try {
+      const { data } &#x3D; await axios
+        .get(&#x60;${process.env.COINGECKO_API}/simple/price&#x60;, {
+          params: {
+            ids: name,
+            vs_currencies: &#x27;usd&#x27;,
+            include_market_cap: true,
+          },
+        })
+        .catch((e) &#x3D;&gt; {
+          throw axiosErrorHandler(e, &#x60;No such asset ${name}&#x60;);
+        });
+
+      return data ? data : null;
+    } catch (e) {
+      throw e;
+    }
+  }
+
+  /**
+   * Get token avg 7 market caps
+   * @param name
+   */
+  public async getTokenAvg7MarketCaps(name: string): Promise&lt;any&gt; {
+    try {
+      const { data: data7 } &#x3D; await axios
+        .get(&#x60;${process.env.COINGECKO_API}/coins/${name}/market_chart&#x60;, {
+          params: { vs_currency: &#x27;usd&#x27;, days: &#x27;7&#x27;, interval: &#x27;daily&#x27; },
+        })
+        .catch((e) &#x3D;&gt; {
+          throw axiosErrorHandler(e, &#x60;No such avg 7 market caps (${name})&#x60;);
+        });
+
+      const data &#x3D; data7.market_caps.pop().pop() || 0;
+
+      return data ? data : null;
+    } catch (e) {
+      throw e;
+    }
+  }
+
+  /**
+   * Get token avg 14 market caps
+   * @param name
+   */
+  public async getTokenAvg14MarketCaps(name: string): Promise&lt;any&gt; {
+    try {
+      const { data: data14 } &#x3D; await axios
+        .get(&#x60;${process.env.COINGECKO_API}/coins/${name}/market_chart&#x60;, {
+          params: { vs_currency: &#x27;usd&#x27;, days: &#x27;14&#x27;, interval: &#x27;daily&#x27; },
+        })
+        .catch((e) &#x3D;&gt; {
+          throw axiosErrorHandler(e, &#x60;No such avg 14 market caps (${name})&#x60;);
+        });
+
+      const data &#x3D; data14.market_caps.pop().pop() || 0;
+
+      return data ? data : null;
+    } catch (e) {
+      throw e;
+    }
+  }
+}
+</code></pre>

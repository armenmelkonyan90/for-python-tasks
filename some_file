+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    try {
+      const { platformId, blockchainId, assetId } &#x3D; body;
+
+      const asset &#x3D; await this._assetsRepository.findOne(assetId);
+      if (!asset) {
+        throw buildResponse(false, { message: &#x27;Asset not found&#x27; });
+      }
+      const blockchain &#x3D; await this._blockchainRepository.findOne(blockchainId);
+      if (!blockchain) {
+        throw buildResponse(false, { message: &#x27;Blockchain not found&#x27; });
+      }
+      const platform &#x3D; await this._platformRepository.findOne(platformId);
+      if (!platform) {
+        throw buildResponse(false, { message: &#x27;Platform not found&#x27; });
+      }
+
+      const connectionExists &#x3D; await this._platformRepository.findOne({
+        where: { id: platformId, blockchainId, assetId },
+      });
+
+      if (!connectionExists) {
+        throw buildResponse(false, {
+          message: &#x27;No such defi shield connection&#x27;,
+        });
+      }
+
+      const defiShieldGeneral &#x3D; await this._defiShieldGeneralRepository.findOne(
+        { platformId, blockchainId, assetId },
+      );
+
+      const newDefiShield &#x3D; await this._defiShieldRepository.save({
+        platformId,
+        blockchainId,
+        assetId,
+        userId,
+        productRisk: defiShieldGeneral ? defiShieldGeneral.productRisk : 0,
+        riskLastUpdated: defiShieldGeneral ? defiShieldGeneral.updatedAt : null,
+      });
+
+      return buildResponse(true, { newDefiShield }) as GeneralResponse;
+    } catch (e) {
+      throw e;
+    }
+  }
+
+  /**
+   * @description This method is used to get DefiShields for user
+   * @param userId
+   */
+  public async getDefiShields(
+    userId: number,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    try {
+      const defiShields &#x3D; await this._defiShieldRepository.find({
+        where: { userId },
+        relations: [
+          &#x27;platformId&#x27;,
+          &#x27;blockchainId&#x27;,
+          &#x27;assetId&#x27;,
+          &#x27;userId&#x27;,
+          &#x27;userId.alertServices&#x27;,
+        ],
+        order: { createdAt: &#x27;DESC&#x27; },
+      });
+
+      const defiShieldsData &#x3D; [];
+      if (!isEmpty(defiShields)) {
+        await Promise.map(defiShields, async (defiShield) &#x3D;&gt; {
+          const defiShieldGeneral &#x3D;
+            await this._defiShieldGeneralRepository.findOne({
+              platformId: defiShield.platformId[&#x27;id&#x27;],
+              blockchainId: defiShield.blockchainId[&#x27;id&#x27;],
+              assetId: defiShield.assetId[&#x27;id&#x27;],
+            });
+
+          await this._defiShieldRepository.update(
+            {
+              platformId: defiShield.platformId[&#x27;id&#x27;],
+              blockchainId: defiShield.blockchainId[&#x27;id&#x27;],
+              assetId: defiShield.assetId[&#x27;id&#x27;],
+            },
+            {
+              productRisk: defiShieldGeneral
+                ? defiShieldGeneral.productRisk
+                : defiShield.productRisk,
+              riskLastUpdated: new Date(),
+            },
+          );
+
+          if (
+            defiShield.autoUnstacking &amp;&amp;
+            defiShield.autoUnstacking &gt;&#x3D;
+              (defiShieldGeneral
+                ? defiShieldGeneral.productRisk
+                : defiShield.productRisk)
+          ) {
+            await this._integrationService.unstake();
+            await this._integrationService.unstake(
+              defiShield.userId[&#x27;id&#x27;],
+              defiShield.platformId.name
+                ? defiShield.platformId.name.toLowerCase()
+                : null,
+            );
+          }
+
+          if (
+            defiShield.alert &amp;&amp;
+            defiShield.alert &gt;&#x3D;
+              (defiShieldGeneral
+                ? defiShieldGeneral.productRisk
+                : defiShield.productRisk)
+          ) {
+            await this.alertByTimes(defiShield, defiShieldGeneral);
+          }
+
+          defiShieldsData.push({
+            ...defiShield,
+            risks: {
+              assetRisk:
+                defiShield.assetId[&#x27;value&#x27;] ||
+                defiShield.assetId[&#x27;defaultValue&#x27;],
+              productRisk: defiShieldGeneral
+                ? defiShieldGeneral.productRisk
+                : defiShield.productRisk,
+              trustScore: this.productTrustScore(defiShield.productRisk),
+              tokenPrice: {
+                usd: defiShield.assetId[&#x27;usdPrice&#x27;] || 0,
+                usd_market_cap: defiShield.assetId[&#x27;usdMarketCap&#x27;] || 0,
+              },
+              lastUpdated: defiShieldGeneral
+                ? defiShieldGeneral.updatedAt
+                : defiShield.riskLastUpdated,
+            },
+          });
+        });
+      }
+
+      return buildResponse(
+        true,
+        isEmpty(defiShieldsData)
+          ? defiShields
+          : orderBy(defiShieldsData, &#x27;createdAt&#x27;, &#x27;desc&#x27;),
+      ) as GeneralResponse;
+    } catch (e) {
+      throw e;
+    }
+  }
+

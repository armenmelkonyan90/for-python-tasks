  }\n  }\n\n  /**\n   * @description get wallet networks\n   * @param token\n   */\n  public async getNetworks(token: string) {\n    try {\n      const data = await this._apiService.request(\n        token,\n        'GET',\n        '/wallets/networks',\n      );\n\n      return buildResponse(true, data);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description create wallet\n   * @param token\n   * @param body\n   */\n  public async create(token: string, body: CreateWalletDto) {\n    try {\n      const data = await this._apiService.request(token, 'POST', '/wallets', {\n        body,\n      });\n\n      return buildResponse(true, data);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description update wallet\n   * @param token\n   * @param id\n   * @param body\n   */\n  public async update(token: string, id: number, body: UpdateWalletDto) {\n    try {\n      const data = await this._apiService.request(\n        token,\n        'PUT',\n        `/wallets/${id}`,\n        { body },\n      );\n\n      return buildResponse(true, data);\n    } catch (e) {\n      throw e;\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"interfaces/WebToken.html":{"url":"interfaces/WebToken.html","title":"interface - WebToken","body":"\n                   \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n  Interfaces\n  \n  WebToken\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/interface/web-token.interface.ts\n        \n\n\n\n\n        \n            Index\n            \n                \n                    \n                        \n                            Properties\n                        \n                    \n                    \n                        \n                            \n                              
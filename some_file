param;\n    return this.defiShieldService.getPlatforms(assetId, blockchainName);\n  }\n\n  /**\n   * @Description Get the defi shield connections\n   */\n  @Get('connections')\n  public async getConnections(): Promise {\n    return this.defiShieldService.getConnections();\n  }\n\n  /**\n   * @Description Create the defi shield asset\n   * @Route POST /defishield/asset\n   * @param body\n   */\n  @Post('asset')\n  @Roles(RoleTypes.SUPERADMIN)\n  public async postAsset(\n    @Body() body: AssetDto,\n  ): Promise {\n    return this.defiShieldService.createAsset(body);\n  }\n\n  /**\n   * @Description Create the defi shield blockchain and platform\n   * @Route POST /defishield/blockchain-platform\n   * @param body\n   */\n  @Post('blockchain-platform')\n  @Roles(RoleTypes.SUPERADMIN)\n  public async postBlockchainAndPlatform(\n    @Body() body: DefiShieldDefaultsDto,\n  ): Promise {\n    return this.defiShieldService.createBlockchainAndPlatform(body);\n  }\n\n  /**\n   * @Description Get the defi shield\n   */\n  @Get()\n  @Roles(RoleTypes.SUPERADMIN)\n  public async getDefiShieldDefaults(): Promise {\n    return this.defiShieldService.getDefiShieldDefaults();\n  }\n\n  /**\n   * @Description Update the defi shield asset\n   * @Param id\n   * @Param body\n   */\n  @Put('asset/:id')\n  @Roles(RoleTypes.SUPERADMIN)\n  public async putAsset(\n    @Param() id: number,\n    @Body() body: UpdateAssetDto,\n  ): Promise {\n    return this.defiShieldService.updateAsset(id, body);\n  }\n\n  /**\n   * @Description Update the defi shield blockchain or platform\n   * @Param body\n   */\n  @Put('blockchain-platform')\n  @Roles(RoleTypes.SUPERADMIN)\n  public async putBlockchainOrPlatform(\n    @Body() body: UpdateDefiShieldDto,\n  ): Promise {\n    return this.defiShieldService.updateBlockchainOrPlatform(body);\n  }\n\n  /**\n   * @Description Delete the defi shield asset\n   * @Param id\n   */\n  @Delete('asset/:id')\n  @Roles(RoleTypes.SUPERADMIN)\n  public async deleteAsset(\n    @Param() id: number,\n  ): Promise {\n    return this.defiShieldService.deleteAsset(id);\n  }\n\n  /**\n   * @Description Delete the defi shield blockchain or platform\n   * @Param query\n   */\n  @Delete('blockchain-platform')\n  @Roles(RoleTypes.SUPERADMIN)\n  public async deleteBlockchainOrPlatform(\n    @Query() query: DeleteDefishieldDto,\n  ): Promise {\n    return this.defiShieldService.deleteBlockchainOrPlatform(query);\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"modules/DefishieldDefaultsModule.html":{"url":"modules/DefishieldDefaultsModule.html","title":"module - DefishieldDefaultsModule","body":"\n                   \n\n\n\n\n    Modules\n    DefishieldDefaultsModule\n\n\n\n    \n        \n\n\n\n\n\ndependencies\n\ndependencies\n\ncluster_DefishieldDefaultsModule\n\n\n\ncluster_DefishieldDefaultsModule_providers\n\n\n\n\nDefishieldDefaultsService\n\nDefishieldDefaultsService\n\n\n\nDefishieldDefaultsModule\n\nDefishieldDefaultsModule\n\nDefishieldDefaultsModule -->\n\nDefishieldDefaultsService->DefishieldDefaultsModule\n\n\n\n\n\n\n    \n    \n    \n        Zoom in\n        Reset\n        Zoom out\n    \n\n\n\n\n\n    \n        Info\n    \n    \n        Source\n    \n\n\n\n    \n\n        \n            File\n        \n        \n            src/resources/defishield/defishield-defaults/defishield-defaults.module.ts\n        \n\n\n\n\n\n        \n            \n                \n                    Providers\n                    \n                        \n                            DefishieldDefaultsService\n                        \n                    \n                \n                \n                    Controllers\n                    \n                        \n                            DefishieldDefaultsController\n                        \n                    \n                \n            \n        \n\n\n    \n
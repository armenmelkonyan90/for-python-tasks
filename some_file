* @param token\n   * @param query\n   */\n  @Get()\n  public findAll(\n    @Headers('authorization') token: string,\n    @Query() query: AlertsQueryDto,\n  ): Promise {\n    return this._alertsService.findAll(token, query);\n  }\n\n  /**\n   * @description create alert service\n   * @param body\n   * @param user\n   */\n  @Post('/service')\n  public createAlertService(\n    @Body() body: CreateAlertServiceDto,\n    @User() user: AuthUserDto,\n  ) {\n    return this._alertsService.createAlertService(body, Number(user.id));\n  }\n\n  /**\n   * @description get alert services\n   * @param user\n   */\n  @Get('/services')\n  public getAlertServices(@User() user: AuthUserDto) {\n    return this._alertsService.getAlertService(Number(user.id));\n  }\n\n  /**\n   * @description update alert service\n   * @param body\n   * @param param\n   * @param user\n   */\n  @Put('/service/:id')\n  public updateAlertService(\n    @Param() param: AlertsParamDto,\n    @Body() body: UpdateAlertServiceDto,\n    @User() user: AuthUserDto,\n  ) {\n    return this._alertsService.updateAlertService(\n      Number(param.id),\n      body,\n      Number(user.id),\n    );\n  }\n\n  /**\n   * @description delete alert service\n   * @param param\n   * @param user\n   */\n  @Delete('/service/:id')\n  public deleteAlertService(\n    @Param() param: AlertsParamDto,\n    @User() user: AuthUserDto,\n  ) {\n    return this._alertsService.deleteAlertService(\n      Number(param.id),\n      Number(user.id),\n    );\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"modules/AlertsModule.html":{"url":"modules/AlertsModule.html","title":"module - AlertsModule","body":"\n                   \n\n\n\n\n    Modules\n    AlertsModule\n\n\n\n    \n        \n\n\n\n\n\ndependencies\n\ndependencies\n\ncluster_AlertsModule\n\n\n\ncluster_AlertsModule_providers\n\n\n\n\nAlertsService\n\nAlertsServi
+
+    <div class="tab-pane fade  tab-source-code" id="c-source">
+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import {
+  Column,
+  CreateDateColumn,
+  Entity,
+  Index,
+  JoinColumn,
+  ManyToOne,
+  PrimaryGeneratedColumn,
+  UpdateDateColumn,
+} from &#x27;typeorm&#x27;;
+import { Users } from &#x27;@common/database/entities&#x27;;
+
+@Entity(&#x27;investment_risks&#x27;)
+export class InvestmentRisks {
+  @PrimaryGeneratedColumn()
+  id: number;
+
+  @Column({ type: &#x27;numeric&#x27;, nullable: true })
+  @Index()
+  investmentRisk: number;
+
+  @JoinColumn({
+    name: &#x27;userId&#x27;,
+    referencedColumnName: &#x27;id&#x27;,
+  })
+  @ManyToOne(() &#x3D;&gt; Users, (user) &#x3D;&gt; user.investmentRisk, {
+    onDelete: &#x27;CASCADE&#x27;,
+    cascade: true,
+  })
+  userId: number;
+
+  @Column({
+    type: &#x27;numeric&#x27;,
+    nullable: true,
+  })
+  alert?: number;
+
+  @CreateDateColumn({
+    name: &#x27;created_at&#x27;,
+    type: &#x27;timestamp&#x27;,
+  })
+  createdAt: Date;
+
+  @UpdateDateColumn({
+    name: &#x27;updated_at&#x27;,
+    type: &#x27;timestamp&#x27;,
+  })
+  updatedAt: Date;
+}
+</code></pre>
+    </div>
+</div>

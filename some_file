      \n\n                    \n                        Returns :         Promise\n\n                    \n                \n            \n        \n    \n    \n        \n            \n                \n                    \n                    \n                        Public\n                        Async\n                        getPlatforms\n                        \n                    \n                \n            \n            \n                \n                    \n                    getPlatforms(assetId: number, blockchainName: string)\n                \n            \n\n\n            \n                \n                    Defined in src/resources/defishield/defishield-defaults/defishield-defaults.service.ts:65\n                \n            \n\n\n            \n                \n\n                    \n                        Parameters :\n                        \n                        \n                            \n                                \n                                    Name\n                                    Type\n                                    Optional\n                                \n                            \n                            \n                                \n                                    assetId\n                                    \n                                                number\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                                \n                                    blockchainName\n                                    \n                                                string\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                            \n                        \n                    \n                    \n                    \n                    \n                        Returns :         Promise\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n    \n        \n            \n                \n                    \n                    \n                        Public\n                        Async\n                        updateAsset\n                        \n                    \n                \n            \n            \n                \n                    \n                    updateAsset(id: number, body: UpdateAssetDto)\n                \n            \n\n\n            \n                \n                    Defined in src/resources/defishield/defishield-defaults/defishield-defaults.service.ts:191\n                \n            \n\n\n            \n                \n\n                    \n                        Parameters :\n                        \n                        \n                            \n                                \n                                    Name\n                                    Type\n                                    Optional\n                                \n                            \n                            \n                                \n                                    id\n                                    \n                                                number\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                                \n                                    body\n                                    \n                                                UpdateAssetDto\n                                    \n\n                                    \n                                        No\
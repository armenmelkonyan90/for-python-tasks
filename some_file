ext: ExecutionContext,\n    next: CallHandler,\n  ): Observable {\n    return next.handle().pipe(\n      map((data: Observable): any => {\n        const serialize = (obj: any): any =>\n          plainToClass(this.Serializer, obj, {\n            excludeExtraneousValues: false,\n          });\n\n        const serialized = serialize(data);\n        if (this.propName) {\n          return { [this.propName]: serialized };\n        }\n\n        return serialized;\n      }),\n    );\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"injectables/S3BucketService.html":{"url":"injectables/S3BucketService.html","title":"injectable - S3BucketService","body":"\n                   \n\n\n\n\n\n\n\n\n\n  Injectables\n  S3BucketService\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/services/s3bucket.service.ts\n        \n\n\n\n\n\n            \n    Index\n    \n        \n                \n                    \n                        Properties\n                    \n                \n                \n                    \n                        \n                            \n                                AWS_S3_BUCKET\n                            \n                            \n                                s3\n                            \n                        \n                    \n                \n\n                \n                    \n                        Methods\n                    \n                \n                \n                    \n                        \n                            \n                                    Public\n                                    Async\n                                removeFile\n                            \n                            \n                                    Private\n  
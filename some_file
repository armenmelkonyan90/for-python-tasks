                                    \n                                                                    JwtService\n                                                        \n                                                  \n                                                    \n                                                            No\n                                                    \n                                                    \n                                                \n                                        \n                                    \n                            \n                    \n                \n            \n        \n\n\n            \n    \n    \n        Methods\n    \n    \n        \n            \n                \n                    \n                    \n                        Public\n                        Async\n                        use\n                        \n                    \n                \n            \n            \n                \n                    \n                    use(req: Request, _: Response, next: NextFunction)\n                \n            \n\n\n            \n                \n                    Defined in src/common/middlewares/verify.middleware.ts:34\n                \n            \n\n\n            \n                \n                    Middleware\n\n\n                    \n                        Parameters :\n                        \n                        \n                            \n                                \n                                    Name\n                                    Type\n                                    Optional\n                                \n                            \n                            \n                                \n                                    req\n                                    \n                                            Request\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                                \n                                    _\n                                    \n                                            Response\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                                \n                                    next\n                                    \n                                            NextFunction\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                            \n                        \n                    \n                    \n                    \n                    \n                        Returns :         any\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n\n\n    \n\n\n    \n        import { Injectable, NestMiddleware } from '@nestjs/common';\nimport { JwtService } from '@nestjs/jwt';\nimport { InjectRepository } from '@nestjs/typeorm';\nimport { NextFunction, Request, Response } from 'express';\nimport { Repository } from 'typeorm';\nimport { Users, VerificationCodes } from '../database/entities';\nimport { buildResponse } from '../helpers';\nimport { UserStatusEnum } from '@common/models';\nimport { startCase } from 'lodash';\n\n/**\n * Verify middleware\n *\n * @export\n * @class VerifyMiddleware\n * @implements {NestMiddleware}\n */\n@Injectable()\nexport class VerifyMiddleware implements NestMiddleware {\n  constructor(\n    @InjectRepository(Users)\n    private _userRepository: Repository,\n    @InjectRepository(VerificationCodes)\n    private _verificationCodes: Repository,\n    private _jwtService: JwtService,\n  ) {}\n\n  /**\n   * Middleware\n   * @param req\n   * @param _\n   * @param next\n   */\n  public async use(req: Request, _: Response, next: NextFunction) {\n    const token: string = req.headers.authorization\n      ?.replace('Bearer', '')\n      ?.trim();\n    if (!token) throw buildResponse(false, ['Token is empty.']);\n\n    try {\n      this._jwtService.verify(token || '');\n      const decodedUser = this._jwtService.decode(token, { json: true });\n      const { id } = decodedUser as Users;\n      const user: Users = await this._userRepository\n        .createQueryBuilder('users')\n        .where('users.id = :id', { id })\n        .innerJoin(\n          'users.verificationCodes',\n          'verificationCodes',\n          'verificationCodes.isVerified = true',\n        )\n        .innerJoin(\n          'users.twoFactorAuth',\n          'twoFactorAuth',\n          'twoFactorAuth.status_id = :status_id',\n          { status_id: UserStatusEnum.VERIFIED },\n        )\n        .getOne();\n\n      if (!user) {\n        throw { msg: `User not found or is not verified` };\n      }\n\n      req.user = user;\n    } catch (e) {\n      const message = e.msg\n        ? startCase(e.msg)\n        : 'Access token is empty or invalid';\n      throw buildResponse(false, { message: [message] }, 401);\n    }\n    next();\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"controllers/WalletController.html":{"url":"controllers/WalletController.html","title":"controller - WalletController","body":"\n                   \n\n\n\n\n\n\n  Controllers\n  WalletController\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/resources/wallet/wallet.controller.ts\n        \n\n      
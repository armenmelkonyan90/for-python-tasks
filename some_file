+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import { HttpException, Injectable } from &#x27;@nestjs/common&#x27;;
+import { InjectRepository } from &#x27;@nestjs/typeorm&#x27;;
+import {
+  DefiShield,
+  DefiShieldGeneral,
+  InvestmentRisks,
+  TwoFactorAuth,
+  UserSettings,
+  VerificationCodes,
+} from &#x27;@common/database/entities&#x27;;
+import { Not, Repository } from &#x27;typeorm&#x27;;
+import { buildResponse } from &#x27;@common/helpers&#x27;;
+import {
+  GeneralResponse,
+  UserStatusEnum,
+  UserVerificationTypesEnum,
+} from &#x27;@common/models&#x27;;
 
 /**
  * AppService
@@ -89,7 +319,61 @@
  * @class AppService
  */
 @Injectable()
-export class AppService {}
+export class AppService {
+  constructor(
+    @InjectRepository(TwoFactorAuth)
+    private readonly _twoFactorAuth: Repository&lt;TwoFactorAuth&gt;,
+    @InjectRepository(VerificationCodes)
+    private readonly _verificationCodes: Repository&lt;VerificationCodes&gt;,
+    @InjectRepository(DefiShield)
+    private readonly _defiShield: Repository&lt;DefiShield&gt;,
+    @InjectRepository(DefiShieldGeneral)
+    private readonly _defiShieldGeneral: Repository&lt;DefiShieldGeneral&gt;,
+    @InjectRepository(InvestmentRisks)
+    private readonly _investmentRisks: Repository&lt;InvestmentRisks&gt;,
+    @InjectRepository(UserSettings)
+    private readonly _userSettings: Repository&lt;UserSettings&gt;,
+  ) {}
+
+  public async reset(
+    table: string,
+  ): Promise&lt;GeneralResponse&lt;string&gt; | HttpException&gt; {
+    try {
+      switch (table) {
+        case &#x27;defiShield&#x27;:
+          await this._defiShield.delete({ id: Not(0) });
+          break;
+        case &#x27;defiShieldGeneral&#x27;:
+          await this._defiShieldGeneral.delete({ id: Not(0) });
+          break;
+        case &#x27;investmentRisks&#x27;:
+          await this._investmentRisks.delete({ id: Not(0) });
+          break;
+        case &#x27;userSettings&#x27;:
+          await this._userSettings.delete({ id: Not(0) });
+          break;
+        case &#x27;2fa&#x27;:
+          await this._twoFactorAuth.update(
+            { qrScanned: true },
+            { qrScanned: false, status_id: UserStatusEnum.PENDING },
+          );
+          break;
+        case &#x27;verificationCodes&#x27;:
+          await this._verificationCodes.update(
+            { type: UserVerificationTypesEnum.GOOGLE },
+            { isVerified: false },
+          );
+          break;
+        default:
+          throw buildResponse(false, &#x27;Table not found&#x27;, 404);
+      }
+
+      return buildResponse(true, &#x60;${table} reseted&#x60;);
+    } catch (e) {
+      throw e;
+    }
+  }
+}
 </code></pre>

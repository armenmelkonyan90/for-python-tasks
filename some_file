} from 'crypto';\n\n/**\n * @description Hashing system\n */\nexport class HashingSystem {\n  private readonly algorithm = `aes-256-ctr`;\n  private readonly iv = randomBytes(16);\n  constructor(private readonly configService: ConfigService) {}\n\n  /**\n   * @description Encrypts a string\n   * @param text\n   * @param password\n   */\n  encrypt(text: string, password: string = null) {\n    if (!text) return text;\n\n    const ENCRYPTION_PASSWORD: string =\n      this.configService?.get('appConfig')?.ENCRYPTION_PASSWORD;\n\n    const cipher: Cipher = createCipheriv(\n      this.algorithm,\n      Buffer.from(\n        ENCRYPTION_PASSWORD ?? process.env.ENCRYPTION_PASSWORD ?? password,\n      ),\n      this.iv,\n    );\n\n    let encrypted = cipher.update(text);\n\n    encrypted = Buffer.concat([encrypted, cipher.final()]);\n\n    return this.iv.toString('hex') + ':' + encrypted.toString('hex');\n  }\n\n  /**\n   * @description Decrypts a string\n   * @param text\n   * @param password\n   */\n  decrypt(text: string, password: string = null) {\n    if (!text) return text;\n\n    const ENCRYPTION_PASSWORD: string =\n      this.configService?.get('appConfig')?.ENCRYPTION_PASSWORD;\n\n    const textParts = text.split(':');\n    const iv = Buffer.from(textParts.shift(), 'hex');\n    const encryptedText = Buffer.from(textParts.join(':'), 'hex');\n    const decipher: Decipher = createDecipheriv(\n      this.algorithm,\n      Buffer.from(\n        ENCRYPTION_PASSWORD ?? process.env.ENCRYPTION_PASSWORD ?? password,\n      ),\n      iv,\n    );\n\n    let decrypted = decipher.update(encryptedText);\n\n    decrypted = Buffer.concat([decrypted, decipher.final()]);\n\n    return decrypted.toString();\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"controllers/HistoryController.html":{"url":"controllers/HistoryController.html","title":"controller - HistoryController"
+  public async saveRiskInDefiShield(): Promise&lt;true | HttpException&gt; {
+    try {
+      const defiShields &#x3D; await this._defiShieldRepository.find({
+        relations: [&#x27;platformId&#x27;, &#x27;blockchainId&#x27;, &#x27;assetId&#x27;],

                          Type\n                                    Optional\n                                \n                            \n                            \n                                \n                                    to\n                                    \n                                                string\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                                \n                                    subject\n                                    \n                                                string\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                                \n                                    html\n                                    \n                                                string\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                            \n                        \n                    \n                    \n                    \n                    \n                        Returns :     Promise\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n\n\n    \n\n\n    \n        import * as nodemailer from 'nodemailer';\nimport { ConfigService } from '@nestjs/config';\nimport { Injectable, Logger } from '@nestjs/common';\n\n/**\n * Email service\n */\n@Injectable()\nexport class EmailService {\n  constructor(private readonly configService: ConfigService) {}\n\n  /**\n   * Send email\n   * @param to\n   * @param subject\n   * @param html\n   */\n  public async sendEmail(\n    to: string,\n    subject: string,\n    html: string,\n  ): Promise {\n    try {\n      const smtpConfig = this.configService.get('emailConfig');\n\n      const transporter = await nodemailer.createTransport(smtpConfig);\n\n      await transporter.verify((error) => {\n        if (error) {\n          Logger.log('Verify: ' + error);\n        } else {\n          Logger.log('Server is ready to take our messages');\n        }\n      });\n\n      const mailOptions = {\n        from: `\"LightCore\" `,\n        to,\n        subject,\n        html,\n      };\n\n      return await transporter.sendMail(mailOptions, (error, info) => {\n        if (error) {\n          return Logger.log(error);\n        }\n        Logger.log('Message sent: ' + info.response);\n        return info.response;\n      });\n    } catch (e) {\n      throw e;\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"interfaces/ErrorResponse.html":{"url":"interfaces/ErrorResponse.html","title":"interface - ErrorResponse","body":"\n                   \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n  Interfaces\n  \n  ErrorResponse\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/models/general-response.ts\n        \n\n\n\n\n        \n            Index\n            \n                \n                    \n                        \n                            Properties\n                        \n                    \n                    \n                        \n                            \n                                \n                                        \n                                            message\n                                        \n                                \n                            \n                      
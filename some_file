am body\n   */\n  @Post('/refresh-token')\n  public async refreshToken(\n    @Body() body: RefreshTokenDto,\n  ): Promise | HttpException> {\n    return await this._authService.refreshToken(body);\n  }\n\n  /**\n   * @description - Login an admin\n   * @param body\n   */\n  @Post('/login/admin')\n  public loginAdmin(\n    @Body() body: LoginDTO,\n  ): Promise | HttpException> {\n    return this._authService.loginAdmin(body);\n  }\n\n  /**\n   * @description - Check 2fa verification code\n   * @param req\n   * @param body\n   */\n  @Post('/verify')\n  public async verifyLogin(\n    @Request() req,\n    @Body() body: CheckVerificationCodeDto,\n  ): Promise {\n    const user: AuthUserDto = req.user;\n    return await this._authService.verifyLogin(body, user);\n  }\n\n  /**\n   * @description - Create a new password\n   * @param body\n   */\n  @Post('/create-password')\n  public createPassword(\n    @Body() body: CreatePasswordDTO,\n  ): Promise {\n    return this._authService.createPassword(body);\n  }\n\n  /**\n   * @description - Change user password\n   * @param user\n   * @param body\n   */\n  @Post('/reset-password')\n  public async resetPassword(\n    @User() user: AuthUserDto,\n    @Body() body: ResetPasswordDto,\n  ): Promise {\n    return await this._authService.resetPassword(Number(user.id), body);\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"injectables/AuthMiddleware.html":{"url":"injectables/AuthMiddleware.html","title":"injectable - AuthMiddleware","body":"\n                   \n\n\n\n\n\n\n\n\n\n  Injectables\n  AuthMiddleware\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/middlewares/auth.middleware.ts\n        \n\n\n            \n                Description\n            \n            \n                A
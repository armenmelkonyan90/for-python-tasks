+
+
+    <div class="tab-pane fade  tab-source-code" id="c-source">
+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import {
+  Body,
+  Controller,
+  Delete,
+  Get,
+  HttpException,
+  Param,
+  Post,
+  Put,
+  Query,
+} from &#x27;@nestjs/common&#x27;;
+import { GeneralResponse, RoleTypes } from &#x27;@common/models&#x27;;
+import { AssetDto, DefiShieldDefaultsDto } from &#x27;./dto/defishield.dto&#x27;;
+import {
+  UpdateAssetDto,
+  UpdateDefiShieldDto,
+} from &#x27;./dto/update-defishield.dto&#x27;;
+import { DeleteDefishieldDto } from &#x27;./dto/delete-defishield.dto&#x27;;
+import { AssetIdDto, GetPlatformsDto } from &#x27;./dto/get-defishield-defaults.dto&#x27;;
+import { DefishieldDefaultsService } from &#x27;./defishield-defaults.service&#x27;;
+import { Roles } from &#x27;@common/decorators&#x27;;
+
+@Controller(&#x27;defaults&#x27;)
+export class DefishieldDefaultsController {
+  constructor(private readonly defiShieldService: DefishieldDefaultsService) {}
+
+  /**
+   * @Description Get the asset list
+   */
+  @Get(&#x27;assets&#x27;)
+  public async getAssets(): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.getAssets();
+  }
+
+  /**
+   * @Description Get the blockchain list
+   * @param param
+   */
+  @Get(&#x27;blockchains/:assetId&#x27;)
+  public async getBlockchains(
+    @Param() param: AssetIdDto,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    const { assetId } &#x3D; param;
+    return this.defiShieldService.getBlockchains(assetId);
+  }
+
+  /**
+   * @Description Get the platform list
+   * @param param
+   */
+  @Get(&#x27;platforms/:assetId/:blockchainName&#x27;)
+  public async getPlatforms(
+    @Param() param: GetPlatformsDto,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    const { assetId, blockchainName } &#x3D; param;
+    return this.defiShieldService.getPlatforms(assetId, blockchainName);
+  }
+
+  /**
+   * @Description Get the defi shield connections
+   */
+  @Get(&#x27;connections&#x27;)
+  public async getConnections(): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.getConnections();
+  }
+
+  /**
+   * @Description Create the defi shield asset
+   * @Route POST /defishield/asset
+   * @param body
+   */
+  @Post(&#x27;asset&#x27;)
+  @Roles(RoleTypes.SUPERADMIN)
+  public async postAsset(
+    @Body() body: AssetDto,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.createAsset(body);
+  }
+
+  /**
+   * @Description Create the defi shield blockchain and platform
+   * @Route POST /defishield/blockchain-platform
+   * @param body
+   */
+  @Post(&#x27;blockchain-platform&#x27;)
+  @Roles(RoleTypes.SUPERADMIN)
+  public async postBlockchainAndPlatform(
+    @Body() body: DefiShieldDefaultsDto,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.createBlockchainAndPlatform(body);
+  }
+
+  /**
+   * @Description Get the defi shield
+   */
+  @Get()
+  @Roles(RoleTypes.SUPERADMIN)
+  public async getDefiShieldDefaults(): Promise&lt;
+    GeneralResponse | HttpException
+  &gt; {
+    return this.defiShieldService.getDefiShieldDefaults();
+  }
+
+  /**
+   * @Description Update the defi shield asset
+   * @Param id
+   * @Param body
+   */
+  @Put(&#x27;asset/:id&#x27;)
+  @Roles(RoleTypes.SUPERADMIN)
+  public async putAsset(
+    @Param() id: number,
+    @Body() body: UpdateAssetDto,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.updateAsset(id, body);
+  }
+
+  /**
+   * @Description Update the defi shield blockchain or platform
+   * @Param body
+   */
+  @Put(&#x27;blockchain-platform&#x27;)
+  @Roles(RoleTypes.SUPERADMIN)
+  public async putBlockchainOrPlatform(
+    @Body() body: UpdateDefiShieldDto,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.updateBlockchainOrPlatform(body);
+  }
+
+  /**
+   * @Description Delete the defi shield asset
+   * @Param id
+   */
+  @Delete(&#x27;asset/:id&#x27;)
+  @Roles(RoleTypes.SUPERADMIN)
+  public async deleteAsset(
+    @Param() id: number,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.deleteAsset(id);
+  }
+
+  /**
+   * @Description Delete the defi shield blockchain or platform
+   * @Param query
+   */
+  @Delete(&#x27;blockchain-platform&#x27;)
+  @Roles(RoleTypes.SUPERADMIN)
+  public async deleteBlockchainOrPlatform(
+    @Query() query: DeleteDefishieldDto,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.deleteBlockchainOrPlatform(query);
+  }
+}

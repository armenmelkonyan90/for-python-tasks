+      const defiShield &#x3D; await this._defiShieldRepository.findOne({
+        id,
+        userId,
       });
-      const platform &#x3D; await this._platformRepository.save({
-        ...body.platform,
-        blockchainId: blockchain.id,
-        assetId: assets.id,
+      if (!defiShield) {
+        throw buildResponse(false, { message: &#x27;Defi shield not found&#x27; });
+      }
+
+      await this._defiShieldRepository.delete(id);

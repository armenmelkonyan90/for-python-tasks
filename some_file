+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import { IsNotEmpty, IsString, MaxLength, MinLength } from &#x27;class-validator&#x27;;
+
+export class AccessPasswordDto {
+  @IsNotEmpty()
+  @IsString({ message: &#x27;Password must be a string&#x27; })
+  @MinLength(8, { message: &#x27;Password should be longer than 8 characters&#x27; })
+  @MaxLength(60, { message: &#x27;Password should be shorter than 60 characters&#x27; })
+  password: string;
+}
+</code></pre>

 \n                        \n                    \n                    \n                    \n                    \n                        Returns :     Promise\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n    \n        \n            \n                \n                    \n                    \n                        Async\n                        unstake\n                        \n                    \n                \n            \n            \n                \n                    \n                    unstake(userId?: number, protocol?: string)\n                \n            \n\n\n            \n                \n                    Defined in src/common/integration-service/integration-service.ts:166\n                \n            \n\n\n            \n                \n                    UNSTAKE SPECIFIED PROTOCOL\n\n\n                    \n                        Parameters :\n                        \n                        \n                            \n                                \n                                    Name\n                                    Type\n                                    Optional\n                                \n                            \n                            \n                                \n                                    userId\n                                    \n                                                number\n                                    \n\n                                    \n                                        Yes\n                                    \n\n\n                                \n                                \n                                    protocol\n                                    \n                                                string\n                                    \n\n                                    \n                                        Yes\n                                    \n\n\n                                \n                            \n                        \n                    \n                    \n                    \n                    \n                        Returns :     unknown\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n\n\n    \n\n\n    \n        import { HashingSystem } from './../hashing-sytem/hashing-system';\nimport { buildResponse } from '@common/helpers';\nimport { Repository } from 'typeorm';\nimport { InjectRepository } from '@nestjs/typeorm';\nimport * as MODELS from './models';\nimport * as ENTITIES from '@common/database/entities';\nimport axios from 'axios';\nimport { Injectable } from '@nestjs/common';\n\nconst INTERNAL_ERROR_MESSAGE = 'Internal Server Error returns your service.';\nconst ERROR_MESSAGE =\n  'Please make sure you have paste correct Client ID and Client Secret on your system and put the correct webhook URL.';\n\n/**\n * Integration service\n */\n@Injectable()\nexport class IntegrationService {\n  constructor(\n    @InjectRepository(ENTITIES.UserSettings)\n    private readonly _usersSettingsRepository: Repository,\n    private readonly _hashingSystem: HashingSystem,\n  ) {}\n\n  /**\n   * POST REQUEST\n   * @description - This method is used to send POST request to the specified url.\n   * @param userId\n   * @param url\n   * @param body\n   * @param headers\n   */\n  async postRequest(\n    userId: number,\n    url: string,\n    body: any,\n    headers = {},\n  ): Promise {\n    try {\n      const userSettings = await this._usersSettingsRepository.findOne({\n        userId,\n      });\n\n      const { data: response } = await axios({\n        baseURL: userSettings\n          ? userSettings.webhookUrl\n          : 'https://j1f0hh5xmf.execute-api.us-east-1.amazonaws.com/dev/account/lightcore',\n        method: 'POST',\n        url,\n        data: userSettings ? body : { protocol: 'aave-avax' },\n        params: { ...headers },\n        headers: { ...headers },\n      }).catch(async (e) => {\n        if (e.response) {\n          const res = e.response;\n          const { status } = res;\n          if (Number(status) >= 500) {\n            await this.increaseFailedAttempts(userId);\n            throw buildResponse(false, {\n              message: [INTERNAL_ERROR_MESSAGE],\n            });\n          } else if (status === 401) {\n            throw buildResponse(false, {\n              message: [ERROR_MESSAGE],\n            });\n          }\n          await this.increaseFailedAttempts(userId);\n\n          throw buildResponse(false, res.data.error, status);\n        }\n        throw buildResponse(false, {\n          message: [INTERNAL_ERROR_MESSAGE],\n        });\n      });\n\n      await this.decreaseFailedAttempts(userId);\n\n      return response;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * GET REQUEST\n   * @description - This method is used to send GET request to the specified url.\n   * @param userId\n   * @param url\n   * @param headers\n   */\n  async getRequest(userId: number, url: string, headers = {}): Promise {\n    try {\n      const userSettings = await this._usersSettingsRepository.findOne({\n        userId,\n      });\n\n      const { data: response } = await axios({\n        baseURL: userSettings\n          ? userSettings.webhookUrl\n          : 'https://j1f0hh5xmf.execute-api.us-east-1.amazonaws.com/dev/account/lightcore',\n        method: 'GET',\n        url,\n        params: { ...headers },\n        headers: { ...headers },\n      }).catch(async (e) => {\n        if (e.response) {\n          const res = e.response;\n          const { status } = res;\n\n          if (Number(status) >= 500) {\n            await this.increaseFailedAttempts(userId);\n            throw buildResponse(false, {\n              message: [INTERNAL_ERROR_MESSAGE],\n            });\n          } else if (status === 401) {\n            throw buildResponse(false, {\n              message: [ERROR_MESSAGE],\n            });\n          }\n          await this.increaseFailedAttempts(userId);\n\n          throw buildResponse(false, res.data.error, status);\n        }\n        throw buildResponse(false, {\n          message: [INTERNAL_ERROR_MESSAGE],\n        });\n      });\n\n      await this.decreaseFailedAttempts(userId);\n\n      return response;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * GET USER SECRET KEYS\n   * @description - This method is used to get user secret keys.\n   * @param userId\n   */\n  async getUserSecretKeys(\n    userId: number,\n  ): Promise {\n    const data = await this._usersSettingsRepository.findOne({ userId });\n\n    if (data) {\n      return {\n        client_id: this._hashingSystem.decrypt(data.clientId),\n        client_secret: this._hashingSystem.decrypt(data.clientSecret),\n      };\n    }\n    return {\n      client_id: '2runjcz1655050155059',\n      client_secret:\n        '7c43ceeb6cc4fac8433faae34bb2ae8e7b114aaef8768dced5599c2244e5a0f59e4a09515ee568a382e03cad178244969835cd6862c982a8333c17b5704f7cbc',\n    };\n  }\n\n  /**\n   * UNSTAKE SPECIFIED PROTOCOL\n   * @description - This method is used to unstake specified protocol. // THE ONLY WORKING protocol(pool) is aave, so far.\n   * @param userId\n   * @param protocol\n   */\n  async unstake(userId?: number, protocol?: string) {\n    const secretKeys = await this.getUserSecretKeys(userId);\n    return this.postRequest(userId, `unstake`, { protocol }, secretKeys);\n  }\n\n  /**\n   * INCREASE FAILED ATTEMPTS\n   * @description - This method is used to increase failed attempts.\n   * @param userId\n   */\n  async increaseFailedAttempts(userId: number): Promise {\n    await this._usersSettingsRepository\n      .createQueryBuilder()\n      .update(ENTITIES.UserSettings)\n      .where({ userId })\n      .set({ errorsCount: () => 'errors_count + 1' })\n      .execute();\n  }\n\n  /**\n   * DECREASE FAILED ATTEMPTS\n   * @description - This method is used to make zero the failed attempts.\n   * @param userId\n   */\n  async decreaseFailedAttempts(userId: number): Promise {\n    await this._usersSettingsRepository.update({ userId }, { errorsCount: 0 });\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"entities/InvestmentRisks.html":{"url":"entities/InvestmentRisks.html","title":"entity - InvestmentRisks","body":"\n                   \n\n\n\n\n\n\n\n  Entities\n  InvestmentRisks\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/database/entities/investment-risks.entity.ts\n        \n\n\n\n\n            \n    Index\n    \n        \n                \n                    \n                        Properties\n                    \n                \n                \n                    \n                        \n                            \n                                    Optional\n                                alert\n                            \n                            \n                                createdAt\n                            \n                            \n                                id\n                            \n                            \n                                investmentRisk\n                            \n                            \n                                updatedAt\n                            \n                            \n                                userId\n                            \n                        \n                    \n                \n\n\n\n\n\n\n        \n    \n\n\n            \n    \n    \n        Properties\n    \n    \n        \n            \n                \n                    \n                    \n        
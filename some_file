          \n                                    Name\n                                    Type\n                                    Optional\n                                \n                            \n                            \n                                \n                                    twoFactorAuthenticationCode\n                                    \n                                                string\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                                \n                                    authUser\n                                    \n                                                AuthUserDto\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                            \n                        \n                    \n                    \n                    \n                    \n                        Returns :     unknown\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n    \n        \n            \n                \n                    \n                    \n                        Public\n                        Async\n                        pipeQrCodeStream\n                        \n                    \n                \n            \n            \n                \n                    \n                    pipeQrCodeStream(otpAuthUrl: string)\n                \n            \n\n\n            \n                \n                    Defined in src/resources/two-factor-authentication/two-factor-authentication.service.ts:80\n                \n            \n\n\n            \n                \n\n                    \n                        Parameters :\n                        \n                        \n                            \n                                \n                                    Name\n                                    Type\n                                    Optional\n                                \n                            \n                            \n                                \n                                    otpAuthUrl\n                                    \n                                                string\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                            \n                        \n                    \n                    \n                    \n                    \n                        Returns :     unknown\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n\n\n    \n\n\n    \n        import { Users } from '@common/database/entities';\nimport { ConfigService } from '@nestjs/config';\nimport { Injectable } from '@nestjs/common';\nimport { authenticator } from 'otplib';\nimport { toDataURL } from 'qrcode';\nimport { JwtService } from '@nestjs/jwt';\nimport { omit } from 'lodash';\nimport { AccessTokenDto, AuthUserDto, AuthUserPayloadDto } from '../auth/dto';\nimport { InjectRepository } from '@nestjs/typeorm';\nimport { Repository } from 'typeorm';\nimport { buildResponse } from '@common/helpers';\nimport { JWT_OPTIONS } from '@common/constants';\nimport {\n  LoginUsersStatusEnum,\n  UserStatusEnum,\n  UserVerificationTypesEnum,\n} from '@common/models';\nimport { TwoFactorAuth } from '@common/database/entities/2fa.entity';\n\n/**\n * Two-factor authentication service\n */\n@Injectable()\nexport class TwoFactorAuthenticationService {\n  constructor(\n    @InjectRepository(Users)\n    private _usersRepository: Repository,\n    @InjectRepository(TwoFactorAuth)\n    private _twoFactorAuth: Repository,\n    private readonly configService: ConfigService,\n    private _jwtService: JwtService,\n  ) {}\n\n  /**\n   * @description Generate a new 2fa secret for the user\n   * @param authUser\n   */\n  public async generateTwoFactorAuthentication(authUser: AuthUserDto) {\n    const user: Users = await this._usersRepository.findOne(\n      authUser ? authUser.id : 0,\n    );\n\n    if (!user) {\n      throw buildResponse(false, { message: ['User not found'] });\n    }\n\n    const twoFactorAuth: TwoFactorAuth = await this._twoFactorAuth.findOne({\n      userId: authUser ? authUser.id : null,\n    });\n\n    if (\n      twoFactorAuth &&\n      twoFactorAuth.verification_type !== UserVerificationTypesEnum.GOOGLE\n    ) {\n      throw buildResponse(false, {\n        message: ['Your verification type is not a 2FA.'],\n      });\n    }\n\n    const twoFactorConfig = this.configService.get(\n      'twoFactorAuthenticationConfig',\n    );\n\n    const otpAuthUrl = authenticator.keyuri(\n      user.email,\n      twoFactorConfig.appName,\n      twoFactorAuth.two_factor_authentication_secret,\n    );\n\n    return {\n      secret: twoFactorAuth.two_factor_authentication_secret,\n      otpAuthUrl,\n    };\n  }\n\n  /**\n   * @description Generate a QR code for the user to scan\n   * @param otpAuthUrl\n   */\n  public async pipeQrCodeStream(otpAuthUrl: string) {\n    return toDataURL(otpAuthUrl);\n  }\n\n  /**\n   * @description This method is used to verify the user's 2FA code\n   * @param twoFactorAuthenticationCode\n   * @param authUser\n   * @private\n   */\n  private async isTwoFactorAuthenticationCodeValid(\n    twoFactorAuthenticationCode: string,\n    authUser: AuthUserDto,\n  ) {\n    const twoFactorAuth: TwoFactorAuth = await this._twoFactorAuth.findOne({\n      userId: authUser.id,\n    });\n\n    return authenticator.verify({\n      token: twoFactorAuthenticationCode,\n      secret: twoFactorAuth.two_factor_authentication_secret,\n    });\n  }\n\n  /**\n   * @description This method is used to log in user with 2FA\n   * @param user\n   * @param isSecondFactorAuthenticated\n   * @private\n   */\n  private async getJwtAccessToken(\n    user: AuthUserDto,\n    isSecondFactorAuthenticated = false,\n  ): Promise {\n    await this._twoFactorAuth.update(\n      { userId: user.id },\n      { status_id: UserStatusEnum.VERIFIED, qrScanned: true },\n    );\n\n    const payload: Partial = {\n      ...omit(user, [\n        'exp',\n        'iat',\n        'password',\n        'two_factor_authentication_secret',\n      ]),\n      isSecondFactorAuthenticated,\n      verified: LoginUsersStatusEnum.ACTIVE,\n      status_id: UserStatusEnum.VERIFIED,\n    };\n\n    const access_token: string = this._jwtService.sign(payload, {\n      secret: JWT_OPTIONS.secretKey,\n      expiresIn: JWT_OPTIONS.expiresIn,\n    });\n\n    const refresh_token: string = this._jwtService.sign(payload, {\n      secret: JWT_OPTIONS.secretKey,\n      expiresIn: JWT_OPTIONS.refreshExpiresIn,\n    });\n\n    await this._usersRepository.update(user.id, {\n      refreshToken: refresh_token,\n    });\n\n    return {\n      access_token,\n      refresh_token,\n      set_verification: LoginUsersStatusEnum.ACTIVE,\n      verification_type: UserVerificationTypesEnum.GOOGLE,\n      status_id: UserStatusEnum.VERIFIED,\n    } as AccessTokenDto;\n  }\n\n  /**\n   * @description This method is used to verify the user's 2FA code.\n   * @param code\n   * @param authUser\n   */\n  public async authenticateTwoFactorCodeAndGenerateToken(\n    code: string,\n    authUser: AuthUserDto,\n  ): Promise {\n    const isCodeValid = await this.isTwoFactorAuthenticationCodeValid(\n      code,\n      authUser,\n    );\n    if (!isCodeValid) {\n      throw buildResponse(false, { message: ['Wrong authentication code'] });\n    }\n\n    return this.getJwtAccessToken(authUser, true);\n  }\n\n  /**\n   * @description This method is used to verify the user's 2FA code.\n   * @param code\n   * @param authUser\n   */\n  public async authenticateTwoFactorCode(\n    code: string,\n    authUser: AuthUserDto,\n  ): Promise {\n    try {\n      const isCodeValid = await this.isTwoFactorAuthenticationCodeValid(\n        code,\n        authUser,\n      );\n\n      if (!isCodeValid) {\n        throw buildResponse(false, { message: ['Wrong 2fa code'] });\n      }\n\n      return true;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to get the user's 2FA status for client side\n   */\n  public async getTwoFactorAuthenticationStatus(userId: number) {\n    try {\n      const twoFactorAuth: TwoFactorAuth = await this._twoFactorAuth\n        .createQueryBuilder('two_factor_auth')\n        .where('two_factor_auth.userId = :userId', { userId })\n        .addSelect(['two_factor_auth.qrScanned'])\n        .select('two_factor_auth.qrScanned')\n        .getOne();\n\n      if (!twoFactorAuth) {\n        throw buildResponse(false, { message: ['User not found'] });\n      }\n\n      return buildResponse(true, {\n        qrScanned: twoFactorAuth.qrScanned,\n      });\n    } catch (e) {\n      throw e;\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"classes/UpdateAlertServiceDto.html":{"url":"classes/UpdateAlertServiceDto.html","title":"class - UpdateAlertServiceDto","body":"\n                   \n\n\n\n\n\n\n\n\n\n\n\n\n\n  Classes\n  UpdateAlertServiceDto\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/resources/alerts/dto/create-alert-service.dto.ts\n        \n\n\n\n\n\n\n            \n    Index\n    \n        \n                \n                    \n                    
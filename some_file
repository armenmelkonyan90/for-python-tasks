-   * @Route POST /defishield/defaults
-   * @Body DefiShieldDto
+   * @Description Create a new defi shield
+   * @Param {DefiShieldDto} body
+   */
+  @Post()
+  public postDefiShield(
+    @Body() body: CreateDefiShieldDto,
+    @User(&#x27;id&#x27;) userId: number,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.createDefiShield(body, userId);
+  }
+
+  /**
+   * @Description Get the defi shield list for the user
+   * @param userId
+   */
+  @Get(&#x27;list&#x27;)
+  public getDefiShields(
+    @User(&#x27;id&#x27;) userId: number,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.getDefiShields(userId);
+  }
+
+  /**
+   * @Description Get all defi shields for the superadmin
+   */
+  @Roles(RoleTypes.SUPERADMIN)
+  @Get(&#x27;list/all/:userId&#x27;)
+  public getAllDefiShields(
+    @Param() params: GetDefiShieldsDto,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    const { userId } &#x3D; params;
+    return this.defiShieldService.getAllDefiShields(userId);
+  }
+
+  /**
+   * @Description Get the defi shield
+   */
+  @Get(&#x27;defaults&#x27;)
+  public async getDefiShieldDefaults(): Promise&lt;
+    GeneralResponse | HttpException
+  &gt; {
+    return this.defiShieldService.getDefiShieldDefaults();
+  }
+
+  /**
+   * @Description Update the defi shield
+   * @Param {UpdateDefiShieldDto} body
+   * @Param {number} id
+   * @Param {number} userId
+   */
+  @Put(&#x27;:id&#x27;)
+  public updateDefiShield(
+    @Param(&#x27;id&#x27;) id: number,
+    @Body() body: UpdateDefiShieldDto,
+    @User(&#x27;id&#x27;) userId: number,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.updateDefiShield(id, body, userId);
+  }
+
+  /**
+   * @Description Delete the defi shield
+   * @Param {number} id
+   * @Param {number} userId
+   */
+  @Delete(&#x27;:id&#x27;)
+  public deleteDefiShield(
+    @Param(&#x27;id&#x27;) id: number,
+    @User(&#x27;id&#x27;) userId: number,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    return this.defiShieldService.deleteDefiShield(id, userId);
+  }
+
+  /**

   ): Promise&lt;GeneralResponse | HttpException&gt; {
     try {
-      const assetName &#x3D; await this._assetsRepository.findOne({
-        name: body.asset.name,
+      const user &#x3D; await this._userRepository.findOne(userId);
+      if (!user) {
+        throw buildResponse(false, { message: &#x27;User not found&#x27; });
+      }
+
+      const defiShields &#x3D; await this._defiShieldRepository.find({
+        where: { userId },
+        relations: [&#x27;platformId&#x27;, &#x27;blockchainId&#x27;, &#x27;assetId&#x27;, &#x27;userId&#x27;],
+        order: { createdAt: &#x27;DESC&#x27; },
+      });
+
+      const defiShieldsData &#x3D; [];
+      if (!isEmpty(defiShields)) {
+        await Promise.map(defiShields, async (defiShield) &#x3D;&gt; {
+          const defiShieldGeneral &#x3D;
+            await this._defiShieldGeneralRepository.findOne({
+              platformId: defiShield.platformId[&#x27;id&#x27;],

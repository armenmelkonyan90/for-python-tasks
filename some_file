             \n                    \n                    productTrustScore(risk: number)\n                \n            \n\n\n            \n                \n                    Defined in src/resources/defishield/defishield.service.ts:151\n                \n            \n\n\n            \n                \n\n                    \n                        Parameters :\n                        \n                        \n                            \n                                \n                                    Name\n                                    Type\n                                    Optional\n                                \n                            \n                            \n                                \n                                    risk\n                                    \n                                                number\n                                    \n\n                                    \n                                        No\n                                    \n\n\n                                \n                            \n                        \n                    \n                    \n                    \n                    \n                        Returns :     \"green\" | \"yellow\" | \"red\"\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n\n\n    \n\n\n    \n        import { HttpException, Injectable } from '@nestjs/common';\nimport { InjectRepository } from '@nestjs/typeorm';\nimport { Assets, Blockchain, Platform } from '@common/database/entities';\nimport { Repository } from 'typeorm';\nimport { Cron, CronExpression } from '@nestjs/schedule';\nimport { axiosErrorHandler, buildResponse } from '@common/helpers';\nimport axios from 'axios';\nimport { map } from 'lodash';\nimport { GeneralResponse } from '@common/models';\nimport { DefiShieldDto } from './dto/defishield.dto';\n\n@Injectable()\nexport class DefiShieldService {\n  constructor(\n    @InjectRepository(Assets)\n    private _assetsRepository: Repository,\n    @InjectRepository(Blockchain)\n    private _blockchainRepository: Repository,\n    @InjectRepository(Platform)\n    private _platformRepository: Repository,\n  ) {}\n\n  /**\n   * Get token price from coingecko\n   * @param name\n   * @private\n   */\n  private async getTokenPrice(name: string): Promise {\n    try {\n      const { data } = await axios\n        .get(`${process.env.COINGECKO_API}/simple/price`, {\n          params: {\n            ids: name,\n            vs_currencies: 'usd',\n            include_market_cap: true,\n          },\n        })\n        .catch((e) => {\n          throw axiosErrorHandler(e, 'No such asset');\n        });\n\n      return data ? data[name] : null;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * Get token avg 714 market caps\n   * @param name\n   * @private\n   */\n  private async getTokenAvg714MarketCaps(name: string): Promise {\n    try {\n      const { data: data7 } = await axios\n        .get(`${process.env.COINGECKO_API}/coins/${name}/market_chart`, {\n          params: { vs_currency: 'usd', days: '7', interval: 'daily' },\n        })\n        .catch((e) => {\n          throw axiosErrorHandler(e, 'No such avg 7 market caps');\n        });\n\n      const { data: data14 } = await axios\n        .get(`${process.env.COINGECKO_API}/coins/${name}/market_chart`, {\n          params: { vs_currency: 'usd', days: '14', interval: 'daily' },\n        })\n        .catch((e) => {\n          throw axiosErrorHandler(e, 'No such avg 14 market caps');\n        });\n\n      const data = {\n        marketCap7Day: data7.market_caps.pop().pop() || 0,\n        marketCap14Day: data14.market_caps.pop().pop() || 0,\n      };\n\n      return data ? data : null;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * Get asset risk\n   * @param currentPrice\n   * @param marketCap\n   * @param tokenAvg714MarketCaps\n   * @param assetBasicRisk\n   * @private\n   */\n  private static getAssetRisk(\n    currentPrice: number,\n    marketCap: number,\n    tokenAvg714MarketCaps: {\n      marketCap7Day: number;\n      marketCap14Day: number;\n    },\n    assetBasicRisk: number,\n  ): number {\n    try {\n      const assetsPriceRisk = (1 - currentPrice) * 1000;\n\n      const assetAvg714HistoricMarketCap: number =\n        (tokenAvg714MarketCaps.marketCap7Day +\n          tokenAvg714MarketCaps.marketCap14Day) /\n        2;\n\n      const assetMarketCapRisk =\n        ((assetAvg714HistoricMarketCap - marketCap) /\n          assetAvg714HistoricMarketCap) *\n        100;\n\n      return (assetBasicRisk -\n        assetsPriceRisk * 0.7 -\n        assetMarketCapRisk * 0.3) as number;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to get the risks\n   * @param assetRisk\n   * @param blockchainRisk\n   * @param platformRisk\n   * @private\n   */\n  private static productRisk(\n    assetRisk: number,\n    blockchainRisk: number,\n    platformRisk: number,\n  ): number {\n    try {\n      if (assetRisk && blockchainRisk && platformRisk) {\n        return (\n          assetRisk * (1 / 3) +\n          blockchainRisk * (1 / 3) +\n          platformRisk * (1 / 3)\n        );\n      }\n\n      return 0;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to get product trust score color\n   * @param risk\n   */\n  public productTrustScore(risk: number) {\n    try {\n      if (risk = 80) return 'green';\n      if (risk = 60) return 'yellow';\n      if (risk  {\n    try {\n      const platforms = await this._platformRepository.find({\n        relations: ['blockchainId', 'blockchainId.assetId'],\n      });\n\n      const data = await Promise.all(\n        map(platforms, async (platform) => {\n          const blockchain = platform['blockchainId'];\n          const asset = blockchain['assetId'];\n\n          const tokenPrice = await this.getTokenPrice(asset.name);\n          const tokenAvg714MarketCaps = await this.getTokenAvg714MarketCaps(\n            asset.name,\n          );\n\n          const assetRisk = DefiShieldService.getAssetRisk(\n            tokenPrice.usd,\n            tokenPrice['usd_market_cap'],\n            tokenAvg714MarketCaps,\n            asset.defaultValue,\n          );\n\n          const productRisk = DefiShieldService.productRisk(\n            asset.value || asset.defaultValue,\n            blockchain['value'] || blockchain['defaultValue'],\n            platform['value'] || platform['defaultValue'],\n          );\n\n          const newProductRisk = DefiShieldService.productRisk(\n            assetRisk || asset.defaultValue,\n            blockchain['value'] || blockchain['defaultValue'],\n            platform['value'] || platform['defaultValue'],\n          );\n\n          await this._assetsRepository.update(asset.id, { value: assetRisk });\n\n          return {\n            assetRisk,\n            productRisk,\n            newProductRisk,\n            trustScore: this.productTrustScore(newProductRisk),\n            tokenPrice,\n            ...platform,\n          };\n        }),\n      );\n\n      return buildResponse(true, data) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description This method is used to create the risks\n   * @param body\n   */\n  public async postDefiShieldDefaults(\n    body: DefiShieldDto,\n  ): Promise {\n    try {\n      const assetName = await this._assetsRepository.findOne({\n        name: body.asset.name,\n      });\n      const assetSymbol = await this._assetsRepository.findOne({\n        symbol: body.asset.symbol,\n      });\n      if (assetName || assetSymbol) {\n        throw buildResponse(false, { message: 'Asset already exists' });\n      }\n\n      const assets = await this._assetsRepository.save(body.asset);\n      const blockchain = await this._blockchainRepository.save({\n        ...body.blockchain,\n        assetId: assets.id,\n      });\n      const platform = await this._platformRepository.save({\n        ...body.platform,\n        blockchainId: blockchain.id,\n        assetId: assets.id,\n      });\n\n      return buildResponse(true, {\n        assets,\n        blockchain,\n        platform,\n      }) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"injectables/EmailService.html":{"url":"injectables/EmailService.html","title":"injectable - EmailService","body":"\n                   \n\n\n\n\n\n\n\n\n\n  Injectables\n  EmailService\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/services/email.service.ts\n        \n\n\n            \n                Description\n            \n            \n                Email service\n\n            \n\n\n\n            \n    Index\n    \n        \n\n                \n                    \n                        Methods\n                    \n                \n                \n                    \n                        \n                            \n                                    Public\n                                    Async\n                                sendEmail\n                            \n                        \n                    \n                \n\n\n\n\n\n        \n    \n\n\n            \n    Constructor\n        \n            \n                \n                    \nconstructor(configService: ConfigService)\n                    \n                \n                        \n                            \n                                Defined in src/common/services/email.service.ts:9\n                            \n                        \n\n           
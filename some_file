+
+
+    <div class="tab-pane fade  tab-source-code" id="c-source">
+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import {
+  IsNotEmpty,
+  IsNotEmptyObject,
+  IsNumber,
+  IsObject,
+  IsString,
+  ValidateNested,
+} from &#x27;class-validator&#x27;;
+import { Type } from &#x27;class-transformer&#x27;;
+
+export class AssetDto {
+  @IsString()
+  @IsNotEmpty()
+  name: string;
+
+  @IsString()
+  @IsNotEmpty()
+  symbol: string;
+
+  @IsNumber()
+  @IsNotEmpty()
+  defaultValue: number;
+}
+
+export class BlockchainDto {
+  @IsString()
+  @IsNotEmpty()
+  name: string;
+
+  @IsNumber()
+  @IsNotEmpty()
+  defaultValue: number;
+}
+
+export class PlatformDto {
+  @IsString()
+  @IsNotEmpty()
+  name: string;
+
+  @IsNumber()
+  @IsNotEmpty()
+  defaultValue: number;
+}
+
+export class DefiShieldDefaultsDto {
+  @IsNumber()
+  @IsNotEmpty()
+  assetId: number;
+
+  @IsObject()
+  @IsNotEmptyObject()
+  @ValidateNested({ each: true })
+  @Type(() &#x3D;&gt; BlockchainDto)
+  blockchain: BlockchainDto;
+
+  @IsObject()
+  @IsNotEmptyObject()
+  @ValidateNested({ each: true })
+  @Type(() &#x3D;&gt; PlatformDto)
+  platform: PlatformDto;
+}
+</code></pre>
+    </div>
+</div>

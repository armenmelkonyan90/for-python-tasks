        \n                        Returns :         any\n\n                    \n                    \n                        \n                    \n                \n            \n        \n    \n    \n\n\n    \n        import {\n  Controller,\n  Get,\n  Post,\n  Body,\n  Patch,\n  UseInterceptors,\n} from '@nestjs/common';\nimport { UserSettingsService } from './user-settings.service';\nimport { CreateUserSettingDto } from './dto/create-user-setting.dto';\nimport { UpdateUserSettingDto } from './dto/update-user-setting.dto';\nimport { User } from '@common/decorators';\nimport * as ENTITIES from '@common/database/entities';\nimport * as GLOBAL_DTOS from '@common/dtos';\nimport { ResponseInterceptor } from '@common/interceptors';\nimport { AccessPasswordDto } from './dto/access-password.dto';\n\n@Controller('settings')\nexport class UserSettingsController {\n  constructor(private readonly userSettingsService: UserSettingsService) {}\n\n  /**\n   * @description: This method is used to access user for integration service.\n   * @param userId\n   * @param accessPasswordDto\n   */\n  @Post('/access-password')\n  public accessPassword(\n    @User('id') userId: number,\n    @Body() accessPasswordDto: AccessPasswordDto,\n  ) {\n    return this.userSettingsService.accessPassword(userId, accessPasswordDto);\n  }\n\n  /**\n   * @description: This method is used to create user settings\n   * @param userId\n   * @param createUserSettingDto\n   */\n  @Post()\n  public async create(\n    @User('id') userId: number,\n    @Body() createUserSettingDto: CreateUserSettingDto,\n  ) {\n    return this.userSettingsService.create(userId, createUserSettingDto);\n  }\n\n  /**\n   * @description: This method is used to get user settings\n   * @param user_id\n   */\n  @Get()\n  @UseInterceptors(new ResponseInterceptor(ENTITIES.UserSettings))\n  public findOne(@User('id') user_id: number) {\n    return this.userSettingsService.findOne(+user_id);\n  }\n\n  /**\n   * @description: This method is used to update user settings\n   * @param userId\n   * @param updateUserSettingDto\n   */\n  @Patch()\n  public update(\n    @User('id') userId: number,\n    @Body() updateUserSettingDto: UpdateUserSettingDto,\n  ) {\n    return this.userSettingsService.update(+userId, updateUserSettingDto);\n  }\n\n  /**\n   * @description: This method is used to unstake specified protocol(pool)\n   * @param userId\n   * @param protocol\n   */\n  @Post('unstake')\n  public async unStake(\n    @User('id') userId: number,\n    @Body() protocol: GLOBAL_DTOS.ProtocolDto,\n  ) {\n    return this.userSettingsService.unStake(userId, protocol);\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"modules/UserSettingsModule.html":{"url":"modules/UserSettingsModule.html","title":"module - UserSettingsModule","body":"\n                   \n\n\n\n\n    Modules\n    UserSettingsModule\n\n\n\n    \n        \n\n\n\n\n\ndependencies\n\ndependencies\n\ncluster_UserSettingsModule\n\n\n\ncluster_UserSettingsModule_providers\n\n\n\n\nIntegrationService\n\nIntegrationService\n\n\n\nUserSettingsModule\n\nUserSettingsModule\n\nUserSettingsModule -->\n\nIntegrationService->UserSettingsModule\n\n\n\n\n\nUserSettingsService\n\nUserSettingsService\n\nUserSettingsModule -->\n\nUserSettingsService->UserSettingsModule\n\n\n\n\n\n\n    \n    \n    \n        Zoom in\n        Reset\n        Zoom out\n    \n\n\n\n\n\n    \n        Info\n    \n    \n        Source\n    \n\n\n\n    \n\n        \n            File\n        \n        \n            src/resources/user/user-settings/user-settings.module.ts\n        \n\n\n\n\n\n        \n            \n                \n                    Providers\n                    \n                        \n                            IntegrationService\n                        \n                        \n                            UserSettingsService\n     
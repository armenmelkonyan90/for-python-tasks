+
+
+    <div class="tab-pane fade  tab-source-code" id="c-source">
+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import {
+  Column,
+  CreateDateColumn,
+  Entity,
+  JoinColumn,
+  ManyToOne,
+  PrimaryGeneratedColumn,
+  UpdateDateColumn,
+} from &#x27;typeorm&#x27;;
+import { Assets, Blockchain, Platform } from &#x27;@common/database/entities&#x27;;
+
+@Entity(&#x27;defi_shield_general&#x27;)
+export class DefiShieldGeneral {
+  @PrimaryGeneratedColumn()
+  id: number;
+
+  @JoinColumn({
+    name: &#x27;assetId&#x27;,
+    referencedColumnName: &#x27;id&#x27;,
+  })
+  @ManyToOne(() &#x3D;&gt; Assets, (asset) &#x3D;&gt; asset.defiShield, {
+    onDelete: &#x27;CASCADE&#x27;,
+    cascade: true,
+  })
+  assetId: number;
+
+  @JoinColumn({
+    name: &#x27;blockchainId&#x27;,
+    referencedColumnName: &#x27;id&#x27;,
+  })
+  @ManyToOne(() &#x3D;&gt; Blockchain, (blockchain) &#x3D;&gt; blockchain.defiShield, {
+    onDelete: &#x27;CASCADE&#x27;,
+    cascade: true,
+  })
+  blockchainId: number;
+
+  @JoinColumn({
+    name: &#x27;platformId&#x27;,
+    referencedColumnName: &#x27;id&#x27;,
+  })
+  @ManyToOne(() &#x3D;&gt; Platform, (platform) &#x3D;&gt; platform.defiShield, {
+    onDelete: &#x27;CASCADE&#x27;,
+    cascade: true,
+  })
+  platformId: number;
+
+  @Column({ type: &#x27;numeric&#x27;, name: &#x27;product_risk&#x27;, default: 0 })
+  productRisk: number;
+
+  @Column({ nullable: true })
+  trustScore?: string;
+
+  @Column({
+    name: &#x27;usd_price&#x27;,
+    type: &#x27;numeric&#x27;,
+    nullable: true,
+  })
+  usdPrice?: number;
+
+  @Column({
+    name: &#x27;usd_market_cap&#x27;,
+    type: &#x27;numeric&#x27;,
+    nullable: true,
+  })
+  usdMarketCap?: number;
+
+  @CreateDateColumn({
+    name: &#x27;created_at&#x27;,
+    type: &#x27;timestamp&#x27;,
+  })
+  createdAt: Date;
+
+  @UpdateDateColumn({
+    name: &#x27;updated_at&#x27;,
+    type: &#x27;timestamp&#x27;,
+  })
+  updatedAt: Date;
+}
+</code></pre>
+    </div>
+</div>

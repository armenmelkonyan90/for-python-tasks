+
+    <div class="tab-pane fade  tab-source-code" id="c-source">
+        <pre class="line-numbers compodoc-sourcecode"><code class="language-typescript">import { Injectable, Logger } from &#x27;@nestjs/common&#x27;;
+import { Users } from &#x27;@common/database/entities&#x27;;
+import { InjectRepository } from &#x27;@nestjs/typeorm&#x27;;
+import { Repository } from &#x27;typeorm&#x27;;
+import { RoleTypes } from &#x27;@common/models&#x27;;
+import * as bcrypt from &#x27;bcrypt&#x27;;
+
+@Injectable()
+export class SeederService {
+  constructor(
+    private readonly logger: Logger,
+    @InjectRepository(Users)
+    private _users: Repository&lt;Users&gt;,
+  ) {}
+
+  async seed() {
+    await this.users()
+      .then((completed) &#x3D;&gt; {
+        this.logger.debug(&#x27;Successfully completed seeding users...&#x27;);
+        Promise.resolve(completed);
+      })
+      .catch((error) &#x3D;&gt; {
+        this.logger.error(&#x27;Failed seeding users...&#x27;);
+        Promise.reject(error);
+      });
+  }
+
+  private async users() {
+    try {
+      const user &#x3D; await this._users.findOne({
+        email: process.env.ADMIN_EMAIL,
+      });
+      this.logger.debug(&#x27;Found admin user...&#x27;);
+      if (user) {
+        await this._users.update(user.id, { role: RoleTypes.SUPERADMIN });
+      } else {
+        const passwordHash &#x3D; process.env.ADMIN_PASSWORD
+          ? await bcrypt.hash(process.env.ADMIN_PASSWORD, 12)
+          : null;
+        await this._users
+          .save({
+            fullName: process.env.ADMIN_NAME,
+            email: process.env.ADMIN_EMAIL,
+            password: passwordHash,
+            phone: Date.now().toString(),
+            role: RoleTypes.SUPERADMIN,
+          })
+          .then((completed) &#x3D;&gt; {
+            this.logger.debug(&#x27;Successfully completed seeding users...&#x27;);
+            Promise.resolve(completed);
+          })
+          .catch((error) &#x3D;&gt; {
+            this.logger.debug(&#x27;Failed seeding users...&#x27;, error);
+            Promise.reject(error);
+          });
+      }
+
+      return true;
+    } catch (e) {
+      throw e;
+    }
+  }
+}
+</code></pre>
+    </div>

        import { HttpException, Injectable } from '@nestjs/common';\nimport { ApiService } from '@common/services';\nimport { buildResponse } from '@common/helpers';\nimport {\n  AlertsQueryDto,\n  CreateAlertServiceDto,\n  UpdateAlertServiceDto,\n} from './dto';\nimport { InjectRepository } from '@nestjs/typeorm';\nimport { Repository } from 'typeorm';\nimport { AlertServices } from '@common/database/entities';\nimport { GeneralResponse } from '@common/models';\n\n@Injectable()\nexport class AlertsService {\n  constructor(\n    @InjectRepository(AlertServices)\n    private _alertsServiceRepo: Repository,\n    private readonly _apiService: ApiService,\n  ) {}\n\n  /**\n   * @description get alerts\n   * @param token\n   * @param query\n   */\n  public async findAll(token: string, query: AlertsQueryDto): Promise {\n    try {\n      const data = await this._apiService.request(token, 'GET', '/alerts', {\n        params: query,\n      });\n\n      return buildResponse(true, data);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description create alert service\n   * @param body\n   * @param userId\n   */\n  public async createAlertService(\n    body: CreateAlertServiceDto,\n    userId: number,\n  ): Promise {\n    try {\n      const alertService = await this._alertsServiceRepo.findOne({\n        receiver: body.receiver,\n        type: body.type,\n        userId,\n      });\n\n      if (alertService) {\n        throw buildResponse(false, {\n          message: [`You already add this ${body.receiver}`],\n        });\n      }\n\n      await this._alertsServiceRepo.save({ ...body, userId });\n\n      return buildResponse(true);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description get alert services\n   * @param userId\n   */\n  public async getAlertService(\n    userId: number,\n  ): Promise | HttpException> {\n    try {\n      const data = await this._alertsServiceRepo.find({ userId });\n\n      return buildResponse(true, data);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description update alert service\n   * @param body\n   * @param userId\n   * @param id\n   */\n  public async updateAlertService(\n    id: number,\n    body: UpdateAlertServiceDto,\n    userId: number,\n  ): Promise {\n    try {\n      const alertService = await this._alertsServiceRepo.findOne({\n        id,\n        userId,\n      });\n\n      if (!alertService) {\n        throw buildResponse(false, {\n          message: [`You don't have alert service with id ${id}`],\n        });\n      }\n\n      await this._alertsServiceRepo.update(id, { ...body });\n\n      return buildResponse(true);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description delete alert service\n   * @param id\n   * @param userId\n   */\n  public async deleteAlertService(\n    id: number,\n    userId: number,\n  ): Promise {\n    try {\n      const alertService = await this._alertsServiceRepo.findOne(id);\n      if (!alertService) {\n        throw buildResponse(false, {\n          message: [`You don't have alert service with id ${id}`],\n        });\n      }\n      await this._alertsServiceRepo.delete({ id, userId });\n\n      return buildResponse(true);\n    } catch (e) {\n      throw e;\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"injectables/ApiService.html":{"url":"injectables/ApiService.html","title":"injectable - ApiService","body":"\n                   \n\n\n\n\n\n\n\n\n\n  Injectables\n  ApiService\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/services/microservice.api.ts\n        \n\n\n            \n                Description\n            \n            \n                Api Service\n\n            \n\n\n\n            \n    Index\n    \n        \n\n             
                    \n                        \n                    \n                \n            \n        \n    \n\n\n    \n\n\n    \n        import { IntegrationService } from '@common/integration-service/integration-service';\nimport { GeneralResponse } from '@common/models/general-response';\nimport { buildResponse } from '@common/helpers';\nimport { Injectable, HttpException } from '@nestjs/common';\nimport { InjectRepository } from '@nestjs/typeorm';\nimport { CreateUserSettingDto } from './dto/create-user-setting.dto';\nimport { UpdateUserSettingDto } from './dto/update-user-setting.dto';\nimport { HashingSystem } from '@common/hashing-sytem/hashing-system';\nimport * as _ from 'lodash';\n// DATABASE\nimport * as ENTITIES from '@common/database/entities';\nimport { Repository } from 'typeorm';\n// -----------------------------------------------------------------------------\n// UTILS\nimport * as UTILS from './utils';\n// -----------------------------------------------------------------------------\nimport * as GLOBAL_DTOS from '@common/dtos';\n\n@Injectable()\nexport class UserSettingsService {\n  constructor(\n    @InjectRepository(ENTITIES.UserSettings)\n    private readonly _usersSettingsRepository: Repository,\n    private readonly _hashingSystem: HashingSystem,\n    private readonly _integrationService: IntegrationService,\n  ) {}\n\n  /**\n   * @description: This method is used to create user settings\n   * @param userId\n   * @param createUserSettingDto\n   */\n  public async create(\n    userId: number,\n    createUserSettingDto: CreateUserSettingDto,\n  ): Promise {\n    try {\n      const { data: checkExists } = await this.findOne(userId);\n      if (checkExists && !_.isEmpty(checkExists)) {\n        throw buildResponse(false, {\n          message: [`User already entered the settings, please use update.`],\n        });\n      }\n\n      const clientId: string = this._hashingSystem.encrypt(\n        UTILS.generateClientId(),\n      );\n      const clientSecret: string = this._hashingSystem.encrypt(\n        UTILS.generateSecret(),\n      );\n\n      await this._usersSettingsRepository.insert({\n        ...createUserSettingDto,\n        userId,\n        clientId,\n        clientSecret,\n      });\n\n      return buildResponse(true) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description: This method is used to get user settings\n   * @param userId\n   */\n  public async findOne(\n    userId: number,\n  ): Promise> {\n    try {\n      if (!userId || isNaN(userId))\n        throw buildResponse(false, { message: ['Invalid ID'] });\n\n      const userSettings: ENTITIES.UserSettings =\n        await this._usersSettingsRepository\n          .createQueryBuilder('user_settings')\n          .where('user_settings.userId = :id')\n          .setParameters({ id: userId })\n          .getOne();\n\n      if (userSettings && !_.isEmpty(userSettings)) {\n        userSettings.clientId = this._hashingSystem.decrypt(\n          userSettings?.clientId,\n        );\n        userSettings.clientSecret = this._hashingSystem.decrypt(\n          userSettings?.clientSecret,\n        );\n      }\n\n      return buildResponse(\n        true,\n        userSettings,\n      ) as GeneralResponse;\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description: This method is used to update user settings\n   * @param userId\n   * @param updateUserSettingDto\n   */\n  public async update(\n    userId: number,\n    updateUserSettingDto: UpdateUserSettingDto,\n  ): Promise {\n    try {\n      const checkExists = await this._usersSettingsRepository.findOne({\n        userId,\n        webhookUrl: updateUserSettingDto.webhookUrl,\n      });\n\n      if (checkExists) {\n        throw buildResponse(false, {\n          message: [`This webhook url already exist.`],\n        });\n      }\n\n      await this._usersSettingsRepository.update(\n        { userId },\n        { ...updateUserSettingDto },\n      );\n\n      return buildResponse(true);\n    } catch (e) {\n      throw e;\n    }\n  }\n\n  /**\n   * @description: This method is used to do request to integration service, which will handle with unstaking.\n   * @param userId\n   * @param body\n   */\n  public async unStake(userId: number, body: GLOBAL_DTOS.ProtocolDto) {\n    try {\n      const { protocol } = body;\n      await this._integrationService.unstake(userId, protocol);\n      return buildResponse(true);\n    } catch (e) {\n      throw e;\n    }\n  }\n}\n\n    \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n                   \n    \n         results matching \"\"\n        \n    \n    \n        No results matching \"\"\n    \n\n"},"entities/Users.html":{"url":"entities/Users.html","title":"entity - Users","body":"\n                   \n\n\n\n\n\n\n\n  Entities\n  Users\n\n\n\n        \n            Info\n        \n        \n            Source\n        \n\n\n\n    \n        \n            File\n        \n        \n            src/common/database/entities/user.entity.ts\n        \n\n\n\n\n            \n    Index\n    \n        \n                \n                    \n                        Properties\n                    \n                \n                \n                    \n                        \n                            \n                                alertServices\n                            \n                            \n                                    Optional\n                                avatar\n                            \n                            \n                                company\n                            \n                            \n                                createdAt\n                            \n                            \n                                email\n                            \n                            \n                                fullName\n                            \n                            \n                                id\n     
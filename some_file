+  /**
+   * @description This method is used to get the defi shield investment data
+   * @param userId
+   * @param cron
+   */
+  @Cron(CronExpression.EVERY_6_HOURS, {
+    name: &#x27;defi-shield-investment-risks&#x27;,
+  })
+  public async getInvestmentRisks(
+    userId: number,
+    cron &#x3D; true,
+  ): Promise&lt;GeneralResponse | HttpException&gt; {
+    try {
+      let filters &#x3D; {};
+      if (userId) filters &#x3D; { userId };
+      const defiShields &#x3D; await this._defiShieldRepository
+        .createQueryBuilder(&#x27;defiShield&#x27;)
+        .innerJoinAndSelect(&#x27;defiShield.userId&#x27;, &#x27;user&#x27;)
+        .innerJoinAndSelect(&#x27;defiShield.platformId&#x27;, &#x27;platform&#x27;)
+        .where(filters)
+        .select((qb) &#x3D;&gt; {
+          return qb
+            .select(&#x27;SUM(defiShield.stakedAmount)&#x27;, &#x27;stakedAmount&#x27;)
+            .from(&#x27;defi_shield&#x27;, &#x27;defiShield&#x27;);
+        }, &#x27;totalInvestment&#x27;)
+        .orderBy(&#x27;defiShield.id&#x27;, &#x27;ASC&#x27;)
+        .getRawMany();
+
+      let investmentsData;
+      if (!isEmpty(defiShields)) {
+        const totalInvestment: number[] &#x3D; [];
+        const investmentProductExists: number[] &#x3D; [];
+        let investmentRiskByStackAmount: number[] &#x3D; [];
+        let user;
+
+        forEach(defiShields, (v) &#x3D;&gt; {
+          investmentProductExists.push(+v.defiShield_product_risk);
+
+          if (v.defiShield_stacked_amount) {
+            totalInvestment.push(+v.defiShield_stacked_amount);
+            const investmentFormula: number &#x3D;
+              (+v.defiShield_stacked_amount / +v.totalInvestment) *
+              +v.defiShield_product_risk;
+            investmentRiskByStackAmount.push(+investmentFormula);
+          }
+
+          if (v.user_id) {
+            user &#x3D; v.user_id;
+          }
+        });
+
+        if (totalInvestment.length !&#x3D;&#x3D; defiShields.length) {
+          investmentRiskByStackAmount &#x3D; [];
+        }
+
+        const investmentRisk: number &#x3D; !isEmpty(investmentRiskByStackAmount)
+          ? sum(investmentRiskByStackAmount)
+          : sum(investmentProductExists) / investmentProductExists.length;
+
+        if (cron) {
+          await this._investmentRiskRepository.save({
+            investmentRisk,
+            userId: userId ? userId : user,
+          });
+        }
+
+        const currentDayInvestmentRisk &#x3D; await this.currentDayInvestmentRisk(
+          userId ? userId : user,
+        );
+
+        const lastMonthInvestmentRisk &#x3D; await this.lastMonthInvestmentRisk(
+          userId ? userId : user,
+        );
+
+        investmentsData &#x3D; {
+          investmentRisk,
+          investmentRiskTrustScore: this.productTrustScore(investmentRisk),
+          currentDayInvestmentRisk,
+          currentDayInvestmentRiskTrustScore: this.productTrustScore(
+            currentDayInvestmentRisk,
+          ),
+          lastMonthInvestmentRisk,
+          lastMonthInvestmentRiskTrustScore: this.productTrustScore(
+            lastMonthInvestmentRisk,
+          ),
+        };
+      }
+
+      return buildResponse(true, investmentsData) as GeneralResponse;
+    } catch (e) {
+      throw e;
+    }
+  }
+
+  /**
+   * @description This method is used to get the current day investment risk
+   * @param userId
+   * @param day
+   * @private
+   */
+  private async currentDayInvestmentRisk(
+    userId: number,
+    day?: number,
+  ): Promise&lt;number&gt; {
+    try {
+      const date &#x3D; new Date();
+      date.setUTCDate(day || date.getUTCDate());
+
+      const investmentRisk &#x3D; await this._investmentRiskRepository.find({
+        where: [
+          {
+            userId,
+            createdAt: Between(
+              new Date(date.setUTCHours(0, 0, 0)),
+              new Date(date.setUTCHours(5, 59, 59)),
+            ),
+          },
+          {
+            userId,
+            createdAt: Between(
+              new Date(date.setUTCHours(6, 0, 0)),
+              new Date(date.setUTCHours(11, 59, 59)),
+            ),
+          },
+          {
+            userId,
+            createdAt: Between(
+              new Date(date.setUTCHours(12, 0, 0)),
+              new Date(date.setUTCHours(17, 59, 59)),
+            ),
+          },
+          {
+            userId,
+            createdAt: Between(
+              new Date(date.setUTCHours(18, 0, 0)),
+              new Date(date.setUTCHours(23, 59, 59)),
+            ),
+          },
+        ],
       });
 
-      return buildResponse(true, {
-        assets,
-        blockchain,
-        platform,
-      }) as GeneralResponse;
+      let currentDayInvestmentRisk &#x3D; null;
+      if (!isEmpty(investmentRisk)) {
+        currentDayInvestmentRisk &#x3D; investmentRisk.reduce((acc, curr) &#x3D;&gt; {
+          return Number(acc) + Number(curr.investmentRisk);
+        }, 0);
+      }
+
+      return currentDayInvestmentRisk
+        ? currentDayInvestmentRisk / investmentRisk.length
+        : 0;
+    } catch (e) {
+      throw e;
+    }
+  }
+
+  /**
+   * @description This method is used to get the last month investment risk
+   * @param userId
+   * @private
+   */
+  private async lastMonthInvestmentRisk(userId: number): Promise&lt;number&gt; {
+    try {
+      let currentDayInvestmentRisk &#x3D; null;
+
+      for (let i &#x3D; 1; i &lt;&#x3D; 30; i++) {
+        const investmentRisk: number &#x3D; await this.currentDayInvestmentRisk(
+          userId,
+          i,
+        );
+
+        if (investmentRisk) {
+          currentDayInvestmentRisk &#x3D;
+            +currentDayInvestmentRisk + +investmentRisk;
+        }
+      }
+
+      return currentDayInvestmentRisk ? currentDayInvestmentRisk / 30 : 0;
     } catch (e) {
       throw e;
     }
